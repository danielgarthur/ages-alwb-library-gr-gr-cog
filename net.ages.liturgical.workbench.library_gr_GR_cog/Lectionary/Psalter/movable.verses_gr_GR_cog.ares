A_Resource_Whose_Name = movable.verses_gr_GR_cog

psLI.tr.d014.prokeimenon.title = "Ψαλμοὶ 24, 27."
psLI.tr.d014.prokeimenon.v1.info = ps_gr_GR_cog.psa24.v13.number
psLI.tr.d014.prokeimenon.v1.text = "Αἱ ψυχαὶ αὐτῶν ἐν ἀγαθοῖς αὐλισθήσονται."
psLI.tr.d014.prokeimenon.v2.info = ps_gr_GR_cog.psa27.v1.number
psLI.tr.d014.prokeimenon.v2.text = "Πρὸς σέ, Κύριε, κεκράξομαι, ὁ Θεός μου."
psLI.tr.d014.alleluia.title = "Ψαλμοὶ 64, 101."
psLI.tr.d014.alleluia.v1.info = ps_gr_GR_cog.psa64.v5.number
psLI.tr.d014.alleluia.v1.text = "Μακάριοι οὓς ἐξελέξω καὶ προσελάβου, Κύριε."
psLI.tr.d014.alleluia.v2.info = ps_gr_GR_cog.psa101.v13.number
psLI.tr.d014.alleluia.v2.text = "Καὶ τὸ μνημόσυνον αὐτῶν εἰς γενεὰν καὶ γενεάν."

psLI.tr.d015.prokeimenon.title = "Ψαλμοὶ 146, 134."
psLI.tr.d015.prokeimenon.v1.info = ps_gr_GR_cog.psa146.v5.number
psLI.tr.d015.prokeimenon.v1.text = "Μέγας ὁ Κύριος ἡμῶν, καὶ μεγάλη ἡ ἰσχὺς αὐτοῦ."
psLI.tr.d015.prokeimenon.v2.info = ps_gr_GR_cog.psa134.v3.number
psLI.tr.d015.prokeimenon.v2.text = "Αἰνεῖτε τὸν Κύριον, ὅτι ἀγαθὸς Κύριος."
psLI.tr.d015.alleluia.title = "Ψαλμὸς 94."
psLI.tr.d015.alleluia.v1.info = ps_gr_GR_cog.psa94.v1.number
psLI.tr.d015.alleluia.v1.text = "Δεῦτε ἀγαλλιασώμεθα τῷ Κυρίῳ."
psLI.tr.d015.alleluia.v2.info = ps_gr_GR_cog.psa94.v3.number
psLI.tr.d015.alleluia.v2.text = "Ὅτι Θεὸς μέγας Κύριος."

//Cheese Wed
psTE.tr.d018.prokeimenon1.title = "Ψαλμὸς 27."
psTE.tr.d018.prokeimenon1.v1.info = ps_gr_GR_cog.psa27.v9.number
psTE.tr.d018.prokeimenon1.v1.text = "Σῶσον, Κύριε, τὸν λαόν σου."
psTE.tr.d018.prokeimenon1.v2.info = ps_gr_GR_cog.psa27.v1.number
psTE.tr.d018.prokeimenon1.v2.text = "Πρὸς σέ, Κύριε, κεκράξομαι, ὁ Θεός μου."
psTE.tr.d018.prokeimenon2.title = "Ψαλμὸς 28."
psTE.tr.d018.prokeimenon2.v1.info = ps_gr_GR_cog.psa28.v11.number
psTE.tr.d018.prokeimenon2.v1.text = "Κύριος ἰσχὺν τῷ λαῷ αὐτοῦ δώσει."
psTE.tr.d018.prokeimenon2.v2.info = ps_gr_GR_cog.psa28.v1.number
psTE.tr.d018.prokeimenon2.v2.text = "Ἐνέγκατε τῷ Κυρίῳ, υἱοὶ Θεοῦ."
psVE.tr.d019.prokeimenon1.title = "Ψαλμὸς 53."
psVE.tr.d019.prokeimenon1.v1.info = ps_gr_GR_cog.psa53.v3.number
psVE.tr.d019.prokeimenon1.v1.text = "Ὁ Θεός, ἐν τῷ ὀνόματί σου σῶσόν με καὶ ἐν τῇ δυνάμει σου κρινεῖς με."
psVE.tr.d019.prokeimenon1.v2.info = ps_gr_GR_cog.psa53.v4.number
psVE.tr.d019.prokeimenon1.v2.text = "Ὁ Θεός, εἰσάκουσον τῆς προσευχῆς μου."
psVE.tr.d019.prokeimenon2.title = "Ψαλμὸς 130."
psVE.tr.d019.prokeimenon2.v1.info = ps_gr_GR_cog.psa130.v3.number
psVE.tr.d019.prokeimenon2.v1.text = "Ἐλπισάτω Ἰσραὴλ ἐπὶ τὸν Κύριον."
psVE.tr.d019.prokeimenon2.v2.info = ps_gr_GR_cog.psa130.v1.number
psVE.tr.d019.prokeimenon2.v2.text = "Κύριε, οὐχ ὑψώθη ἡ καρδία μου."

//Cheese Fri
psTE.tr.d020.prokeimenon1.title = "Ψαλμὸς 32."
psTE.tr.d020.prokeimenon1.v1.info = ps_gr_GR_cog.psa32.v22.number
psTE.tr.d020.prokeimenon1.v1.text = "Γένοιτο, Κύριε, τὸ ἔλεός σου ἐφʼ ἡμᾶς."
psTE.tr.d020.prokeimenon1.v2.info = ps_gr_GR_cog.psa32.v1.number
psTE.tr.d020.prokeimenon1.v2.text = "Ἀγαλλιᾶσθε, δίκαιοι, ἐν Κυρίῳ."
psTE.tr.d020.prokeimenon2.title = "Ψαλμὸς 46."
psTE.tr.d020.prokeimenon2.v1.info = ps_gr_GR_cog.psa46.v7.number
psTE.tr.d020.prokeimenon2.v1.text = "Ψάλατε τῷ Θεῷ ἡμῶν, ψάλατε."
psTE.tr.d020.prokeimenon2.v2.info = ps_gr_GR_cog.psa46.v2.number
psTE.tr.d020.prokeimenon2.v2.text = "Πάντα τὰ ἔθνη κροτήσατε χεῖρας."
psVE.tr.d021.prokeimenon1.title = "Ψαλμὸς 58."
psVE.tr.d021.prokeimenon1.v1.info = ps_gr_GR_cog.psa58.v10.number
psVE.tr.d021.prokeimenon1.v1.text = "Ὁ Θεός, ἀντιλήπτωρ μου εἶ· τὸ ἔλεός σου προφθάσει με."
psVE.tr.d021.prokeimenon1.v2.info = ps_gr_GR_cog.psa58.v2.number
psVE.tr.d021.prokeimenon1.v2.text = "Ἐξελοῦ με ἐκ τῶν ἐχθρῶν μου, ὁ Θεός."
psVE.tr.d021.prokeimenon2.title = "Ψαλμὸς 130."
psVE.tr.d021.prokeimenon2.v1.info = ps_gr_GR_cog.psa130.v3.number
psVE.tr.d021.prokeimenon2.v1.text = "Ἐλπισάτω Ἰσραὴλ ἐπὶ τὸν Κύριον, ἀπὸ τοῦ νῦν καὶ ἕως τοῦ αἰῶνος."
psVE.tr.d021.prokeimenon2.v2.info = ps_gr_GR_cog.psa130.v1.number
psVE.tr.d021.prokeimenon2.v2.text = "Κύριε, οὐχ ὑψώθη ἡ καρδία μου."

psLI.tr.d021.prokeimenon.title = "Ψαλμὸς 149."
psLI.tr.d021.prokeimenon.v1.info = ps_gr_GR_cog.psa149.v5.number
psLI.tr.d021.prokeimenon.v1.text = "Καυχήσονται ὅσιοι ἐν δόξῃ."
psLI.tr.d021.prokeimenon.v2.info = ps_gr_GR_cog.psa149.v6.number
psLI.tr.d021.prokeimenon.v2.text = "Αἱ ὑψώσεις τοῦ Θεοῦ ἐν τῷ λάρυγγι αὐτῶν."
psLI.tr.d021.alleluia.title = "Ψαλμοὶ 91, 32."
psLI.tr.d021.alleluia.v1.info = ps_gr_GR_cog.psa91.v14.number
psLI.tr.d021.alleluia.v1.text = "Πεφυτευμένοι ἐν τῷ οἴκῳ Κυρίου, ἐν ταῖς αὐλαῖς τοῦ Θεοῦ ἡμῶν ἐξανθήσουσιν."
psLI.tr.d021.alleluia.v2.info = ps_gr_GR_cog.psa32.v1.number
psLI.tr.d021.alleluia.v2.text = "Ἀγαλλιᾶσθε, δίκαιοι, ἐν Κυρίῳ· τοῖς εὐθέσι πρέπει αἴνεσις."

psLI.tr.d022.prokeimenon.title = "Ψαλμὸς 75."
psLI.tr.d022.prokeimenon.v1.info = ps_gr_GR_cog.psa75.v12.number
psLI.tr.d022.prokeimenon.v1.text = "Εὔξασθε καὶ ἀπόδοτε Κυρίῳ τῷ Θεῷ ἡμῶν."
psLI.tr.d022.prokeimenon.v2.info = ps_gr_GR_cog.psa75.v2.number
psLI.tr.d022.prokeimenon.v2.text = "Γνωστὸς ἐν τῇ Ἰουδαίᾳ ὁ Θεός, ἐν τῷ Ἰσραὴλ μέγα τὸ ὄνομα αὐτοῦ."
psLI.tr.d022.alleluia.title = "Ψαλμὸς 91."
psLI.tr.d022.alleluia.v1.info = ps_gr_GR_cog.psa91.v2.number
psLI.tr.d022.alleluia.v1.text = "Ἀγαθὸν τὸ ἐξομολογεῖσθαι τῷ Κυρίῳ καὶ ψάλλειν τῷ ὀνόματί σου, Ὕψιστε."
psLI.tr.d022.alleluia.v2.info = ps_gr_GR_cog.psa91.v3.number
psLI.tr.d022.alleluia.v2.text = "Τοῦ ἀναγγέλλειν τῷ πρωΐ τὸ ἔλεός σου καὶ τὴν ἀλήθειάν σου κατὰ νύκτα."

//Mon 1
psTE.tr.d023.prokeimenon1.title = "Ψαλμὸς 1."
psTE.tr.d023.prokeimenon1.v1.info = ps_gr_GR_cog.psa1.v6.number
psTE.tr.d023.prokeimenon1.v1.text = "Γινώσκει Κύριος ὁδὸν δικαίων, καὶ ὁδὸς ἀσεβῶν ἀπολεῖται."
psTE.tr.d023.prokeimenon1.v2.info = ps_gr_GR_cog.psa1.v1.number
psTE.tr.d023.prokeimenon1.v2.text = "Μακάριος ἀνήρ, ὃς οὐκ ἐπορεύθη ἐν βουλῇ ἀσεβῶν."
psTE.tr.d023.prokeimenon2.title = "Ψαλμὸς 2."
psTE.tr.d023.prokeimenon2.v1.info = ps_gr_GR_cog.psa2.v11.number
psTE.tr.d023.prokeimenon2.v1.text = "Δουλεύσατε τῷ Κυρίῳ ἐν φόβῳ καὶ ἀγαλλιᾶσθε αὐτῷ ἐν τρόμῳ."
psTE.tr.d023.prokeimenon2.v2.info = ps_gr_GR_cog.psa2.v1.number
psTE.tr.d023.prokeimenon2.v2.text = "Ἰνατί ἐφρύαξαν ἔθνη, καὶ λαοὶ ἐμελέτησαν κενά;"
psVE.tr.d024.prokeimenon1.title = "Ψαλμὸς 3."
psVE.tr.d024.prokeimenon1.v1.info = ps_gr_GR_cog.psa3.v9.number
psVE.tr.d024.prokeimenon1.v1.text = "Τοῦ Κυρίου ἡ σωτηρία, καὶ ἐπὶ τὸν λαόν σου ἡ εὐλογία σου."
psVE.tr.d024.prokeimenon1.v2.info = ps_gr_GR_cog.psa3.v2.number
psVE.tr.d024.prokeimenon1.v2.text = "Κύριε, τί ἐπληθύνθησαν οἱ θλίβοντές με;"
psVE.tr.d024.prokeimenon2.title = "Ψαλμὸς 4."
psVE.tr.d024.prokeimenon2.v1.info = ps_gr_GR_cog.psa4.v4.number
psVE.tr.d024.prokeimenon2.v1.text = "Κύριος εἰσακούσεταί μου ἐν τῷ κεκραγέναι με πρὸς αὐτόν."
psVE.tr.d024.prokeimenon2.v2.info = ps_gr_GR_cog.psa4.v2.number
psVE.tr.d024.prokeimenon2.v2.text = "Ἐν τῷ ἐπικαλεῖσθαί με εἰσήκουσάς μου, ὁ Θεὸς τῆς δικαιοσύνης μου."

//Tue 1
psTE.tr.d024.prokeimenon1.title = "Ψαλμὸς 5."
psTE.tr.d024.prokeimenon1.v1.info = ps_gr_GR_cog.psa5.v3.number
psTE.tr.d024.prokeimenon1.v1.text = "Πρόσχες τῇ φωνῇ τῆς δεήσεώς μου."
psTE.tr.d024.prokeimenon1.v2.info = ps_gr_GR_cog.psa5.v2.number
psTE.tr.d024.prokeimenon1.v2.text = "Τὰ ῥήματά μου ἐνώτισαι, Κύριε."
psTE.tr.d024.prokeimenon2.title = "Ψαλμὸς 6."
psTE.tr.d024.prokeimenon2.v1.info = ps_gr_GR_cog.psa6.v2.number
psTE.tr.d024.prokeimenon2.v1.text = "Κύριε, μὴ τῷ θυμῷ σου ἐλέγξῃς με."
psTE.tr.d024.prokeimenon2.v2.info = ps_gr_GR_cog.psa6.v3.number
psTE.tr.d024.prokeimenon2.v2.text = "Ἐλέησόν με, Κύριε, ὅτι ἀσθενής εἰμι."
psVE.tr.d025.prokeimenon1.title = "Ψαλμὸς 7."
psVE.tr.d025.prokeimenon1.v1.info = ps_gr_GR_cog.psa7.v2.number
psVE.tr.d025.prokeimenon1.v1.text = "Κύριε ὁ Θεός μου, ἐπὶ σοὶ ἤλπισα."
psVE.tr.d025.prokeimenon1.v2.info = ps_gr_GR_cog.psa7.v2.number
psVE.tr.d025.prokeimenon1.v2.text = "Σῶσόν με ἐκ πάντων τῶν διωκόντων με."
psVE.tr.d025.prokeimenon2.title = "Ψαλμὸς 8."
psVE.tr.d025.prokeimenon2.v1.info = ps_gr_GR_cog.psa8.v2.number
psVE.tr.d025.prokeimenon2.v1.text = "Κύριε ὁ Κύριος ἡμῶν, ὡς θαυμαστὸν τὸ Ὄνομά σου!"
psVE.tr.d025.prokeimenon2.v2.info = ps_gr_GR_cog.psa8.v2.number
psVE.tr.d025.prokeimenon2.v2.text = "Ὅτι ἐπήρθη ἡ μεγαλοπρέπειά σου ὑπεράνω τῶν Οὐρανῶν."

//Wed 1
psTE.tr.d025.prokeimenon1.title = "Ψαλμὸς 9."
psTE.tr.d025.prokeimenon1.v1.info = ps_gr_GR_cog.psa9.v2.number
psTE.tr.d025.prokeimenon1.v1.text = "Ἐξομολογήσομαί σοι, Κύριε, ἐν ὅλῃ καρδίᾳ μου."
psTE.tr.d025.prokeimenon1.v2.info = ps_gr_GR_cog.psa9.v3.number
psTE.tr.d025.prokeimenon1.v2.text = "Εὐφρανθήσομαι καὶ ἀγαλλιάσομαι ἐν σοί."
psTE.tr.d025.prokeimenon2.title = "Ψαλμὸς 10."
psTE.tr.d025.prokeimenon2.v1.info = ps_gr_GR_cog.psa10.v7.number
psTE.tr.d025.prokeimenon2.v1.text = "Δίκαιος Κύριος, καὶ δικαιοσύνας ἠγάπησεν."
psTE.tr.d025.prokeimenon2.v2.info = ps_gr_GR_cog.psa10.v1.number
psTE.tr.d025.prokeimenon2.v2.text = "Ἐπὶ τῷ Κυρίῳ πέποιθα· πῶς ἐρεῖτε τῇ ψυχῇ μου."
psVE.tr.d026.prokeimenon1.title = "Ψαλμὸς 11."
psVE.tr.d026.prokeimenon1.v1.info = ps_gr_GR_cog.psa11.v8.number
psVE.tr.d026.prokeimenon1.v1.text = "Σύ, Κύριε, φυλάξαις ἡμᾶς καὶ διατηρήσαις ἡμᾶς."
psVE.tr.d026.prokeimenon1.v2.info = ps_gr_GR_cog.psa11.v2.number
psVE.tr.d026.prokeimenon1.v2.text = "Σῶσόν με, Κύριε, ὅτι ἐκλέλοιπεν ὅσιος."
psVE.tr.d026.prokeimenon2.title = "Ψαλμὸς 12."
psVE.tr.d026.prokeimenon2.v1.info = ps_gr_GR_cog.psa12.v4.number
psVE.tr.d026.prokeimenon2.v1.text = "Ἐπίβλεψον, εἰσάκουσόν μου, Κύριε ὁ Θεός μου."
psVE.tr.d026.prokeimenon2.v2.info = ps_gr_GR_cog.psa12.v2.number
psVE.tr.d026.prokeimenon2.v2.text = "Ἔως πότε, Κύριε, ἐπιλήσῃ μου εἰς τέλος;"

//Thu 1
psTE.tr.d026.prokeimenon1.title = "Ψαλμὸς 13."
psTE.tr.d026.prokeimenon1.v1.info = ps_gr_GR_cog.psa13.v7.number
psTE.tr.d026.prokeimenon1.v1.text = "Ἐν τῷ ἐπιστρέψαι Κύριον τὴν αἰχμαλωσίαν τοῦ λαοῦ αὐτοῦ."
psTE.tr.d026.prokeimenon1.v2.info = ps_gr_GR_cog.psa13.v1.number
psTE.tr.d026.prokeimenon1.v2.text = "Εἶπεν ἄφρων ἐν καρδίᾳ αὐτοῦ· Οὐκ ἔστι Θεός."
psTE.tr.d026.prokeimenon2.title = "Ψαλμὸς 14."
psTE.tr.d026.prokeimenon2.v1.info = ps_gr_GR_cog.psa14.v1.number
psTE.tr.d026.prokeimenon2.v1.text = "Κύριε, τίς παροικήσει ἐν τῷ σκηνώματί σου;"
psTE.tr.d026.prokeimenon2.v2.info = ps_gr_GR_cog.psa14.v2.number
psTE.tr.d026.prokeimenon2.v2.text = "Πορευόμενος ἄμωμος καὶ ἐργαζόμενος δικαιοσύνην."
psVE.tr.d027.prokeimenon1.title = "Ψαλμὸς 15."
psVE.tr.d027.prokeimenon1.v1.info = ps_gr_GR_cog.psa15.v7.number
psVE.tr.d027.prokeimenon1.v1.text = "Εὐλογήσω τὸν Κύριον τὸν συνετίσαντά με."
psVE.tr.d027.prokeimenon1.v2.info = ps_gr_GR_cog.psa15.v1.number
psVE.tr.d027.prokeimenon1.v2.text = "Φύλαξόν με, Κύριε, ὅτι ἐπὶ σοὶ ἤλπισα."
psVE.tr.d027.prokeimenon2.title = "Ψαλμὸς 16."
psVE.tr.d027.prokeimenon2.v1.info = ps_gr_GR_cog.psa16.v8.number
psVE.tr.d027.prokeimenon2.v1.text = "Φύλαξόν με ὡς κόρην ὀφθαλμοῦ."
psVE.tr.d027.prokeimenon2.v2.info = ps_gr_GR_cog.psa16.v1.number
psVE.tr.d027.prokeimenon2.v2.text = "Εἰσάκουσον, Κύριε, τῆς δικαιοσύνης μου."

//Fri 1
psTE.tr.d027.prokeimenon1.title = "Ψαλμὸς 17."
psTE.tr.d027.prokeimenon1.v1.info = ps_gr_GR_cog.psa17.v2.number
psTE.tr.d027.prokeimenon1.v1.text = "Ἀγαπήσω σε, Κύριε, ἡ ἰσχύς μου."
psTE.tr.d027.prokeimenon1.v2.info = ps_gr_GR_cog.psa17.v3.number
psTE.tr.d027.prokeimenon1.v2.text = "Ὁ Θεός μου, βοηθός μου, ἐλπιῶ ἐπʼ αὐτόν."
psTE.tr.d027.prokeimenon2.title = "Ψαλμὸς 18."
psTE.tr.d027.prokeimenon2.v1.info = ps_gr_GR_cog.psa18.v15.number
psTE.tr.d027.prokeimenon2.v1.text = "Κύριε, βοηθέ μου καὶ λυτρωτά μου."
psTE.tr.d027.prokeimenon2.v2.info = ps_gr_GR_cog.psa18.v2.number
psTE.tr.d027.prokeimenon2.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psVE.tr.d028.prokeimenon1.title = "Ψαλμὸς 19."
psVE.tr.d028.prokeimenon1.v1.info = ps_gr_GR_cog.psa19.v2.number
psVE.tr.d028.prokeimenon1.v1.text = "Ἐπακούσαι σου Κύριος ἐν ἡμέρᾳ θλίψεως."
psVE.tr.d028.prokeimenon1.v2.info = ps_gr_GR_cog.psa19.v2.number
psVE.tr.d028.prokeimenon1.v2.text = "Υπερασπίσαι σου τὸ Ὄνομα τοῦ Θεοῦ Ἰακώβ."
psVE.tr.d028.prokeimenon2.title = "Ψαλμὸς 20."
psVE.tr.d028.prokeimenon2.v1.info = ps_gr_GR_cog.psa20.v14.number
psVE.tr.d028.prokeimenon2.v1.text = "Ὑψώθητι, Κύριε, ἐν τῇ δυνάμει σου."
psVE.tr.d028.prokeimenon2.v2.info = ps_gr_GR_cog.psa20.v2.number
psVE.tr.d028.prokeimenon2.v2.text = "Κύριε, ἐν τῇ δυνάμει σου εὐφρανθήσεται ὁ βασιλεύς."

psLI.tr.d028.prokeimenon.title = "Ψαλμὸς 63."
psLI.tr.d028.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.tr.d028.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ καὶ ἐλπιεῖ ἐπʼ αὐτόν."
psLI.tr.d028.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.tr.d028.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου, ἐν τῷ δέεσθαί με πρὸς σέ."

psLI.tr.d029.antiphon1.title = "Ψαλμοὶ 92, 105, 106."
psLI.tr.d029.antiphon1.v1.info = ps_gr_GR_cog.psa92.v1.number
psLI.tr.d029.antiphon1.v1.text = "Ὁ Κύριος ἐβασίλευσεν, εὐπρέπειαν ἐνεδύσατο, ἐνεδύσατο Κύριος δύναμιν καὶ περιεζώσατο."
psLI.tr.d029.antiphon1.v2.info = ps_gr_GR_cog.psa92.v1.number
psLI.tr.d029.antiphon1.v2.text = "Καὶ γὰρ ἐστερέωσε τὴν οἰκουμένην, ἥτις οὐ σαλευθήσεται."
psLI.tr.d029.antiphon1.v3.info = ps_gr_GR_cog.psa105.v2.number
psLI.tr.d029.antiphon1.v3.text = "Τίς λαλήσει τὰς δυναστείας τοῦ Κυρίου, ἀκουστὰς ποιήσει πάσας τὰς αἰνέσεις αὐτοῦ;"
psLI.tr.d029.antiphon1.v4.info = ps_gr_GR_cog.psa106.v2.number
psLI.tr.d029.antiphon1.v4.text = "Εἰπάτωσαν οἱ λελυτρωμένοι ὑπὸ Κυρίου, οὓς ἐλυτρώσατο ἐκ χειρὸς ἐχθροῦ."
psLI.tr.d029.antiphon2.title = "Ψαλμοὶ 106, 32, 101."
psLI.tr.d029.antiphon2.v1.info = ps_gr_GR_cog.psa106.v8.number
psLI.tr.d029.antiphon2.v1.text = "Ἐξομολογησάσθωσαν τῷ Κυρίῳ τὰ ἐλέη αὐτοῦ καὶ τὰ θαυμάσια αὐτοῦ τοῖς υἱοῖς τῶν ἀνθρώπων."
psLI.tr.d029.antiphon2.v2.info = ps_gr_GR_cog.psa106.v32.number
psLI.tr.d029.antiphon2.v2.text = "Ὑψωσάτωσαν αὐτὸν ἐν ἐκκλησίᾳ λαοῦ καὶ ἐν καθέδρᾳ πρεσβυτέρων αἰνεσάτωσαν αὐτόν."
psLI.tr.d029.antiphon2.v3.info = ps_gr_GR_cog.psa32.v18.number
psLI.tr.d029.antiphon2.v3.text = "Ὀφθαλμοὶ Κυρίου ἐπὶ τοὺς ἐλπίζοντας ἐπὶ τὸ ἔλεος αὐτοῦ."
psLI.tr.d029.antiphon2.v4.info = ps_gr_GR_cog.psa101.v21.number
psLI.tr.d029.antiphon2.v4.text = "Τοῦ ἀκοῦσαι τοῦ στεναγμοῦ τῶν πεπεδημένων, τοῦ λῦσαι τοὺς υἱοὺς τῶν τεθανατωμένων."
psLI.tr.d029.antiphon3.title = "Ψαλμοὶ 68, 117, 29."
psLI.tr.d029.antiphon3.v1.info = ps_gr_GR_cog.psa68.v35.number
psLI.tr.d029.antiphon3.v1.text = "Αἰνεσάτωσαν αὐτὸν οἱ οὐρανοὶ καὶ ἡ γῆ."
psLI.tr.d029.antiphon3.v2.info = ps_gr_GR_cog.psa117.v24.number
psLI.tr.d029.antiphon3.v2.text = "Αὕτη ἡ ἡμέρα, ἣν ἐποίησεν ὁ Κύριος· ἀγαλλιασώμεθα καὶ εὐφρανθῶμεν ἐν αὐτῇ."
psLI.tr.d029.antiphon3.v3.info = ps_gr_GR_cog.psa29.v13.number
psLI.tr.d029.antiphon3.v3.text = "Κύριε ὁ Θεός μου, εἰς τὸν αἰῶνα ἐξομολογήσομαί σοι."
psLI.tr.d029.prokeimenon.title = le.pr.os_gr_GR_cog.leosLI.pe.d113.EpistleProkeimenon.title
psLI.tr.d029.prokeimenon.v1.info = le.pr.os_gr_GR_cog.leosLI.pe.d113.EpistleProkeimenon.chapverse
psLI.tr.d029.prokeimenon.v1.text = "Εὐλογητὸς εἶ Κύριε ὁ Θεὸς τῶν πατέρων ἡμῶν, καὶ αἰνετὸν καὶ δεδοξασμένον τὸ ὄνομά σου εἰς τοὺς αἰῶνας."
psLI.tr.d029.prokeimenon.v2.info = le.pr.os_gr_GR_cog.leosLI.pe.d113.EpistleProkeimenon.chapverse
psLI.tr.d029.prokeimenon.v2.text = "Ὅτι δίκαιος εἶ ἐπὶ πᾶσιν, οἷς ἐποίησας ἡμῖν, καὶ ἐπὶ τὴν πόλιν τὴν ἁγίαν τὴν τῶν Πατέρων ἡμῶν Ἱερουσαλήμ."
psLI.tr.d029.alleluia.title = "Ψαλμὸς 98."
psLI.tr.d029.alleluia.v1.info = ps_gr_GR_cog.psa98.v6.number
psLI.tr.d029.alleluia.v1.text = "Μωυσῆς καὶ Ἀαρὼν ἐν τοῖς ἱερεῦσιν αὐτοῦ, καὶ Σαμουὴλ ἐν τοῖς ἐπικαλουμένοις τὸ ὄνομα αὐτοῦ."
psLI.tr.d029.alleluia.v2.info = ps_gr_GR_cog.psa98.v6.number
psLI.tr.d029.alleluia.v2.text = "Ἐπεκαλοῦντο τὸν Κύριον, καὶ αὐτὸς εἰσήκουσεν αὐτῶν."

//Mon 2
psTE.tr.d030.prokeimenon1.title = "Ψαλμὸς 21."
psTE.tr.d030.prokeimenon1.v1.info = ps_gr_GR_cog.psa21.v24.number
psTE.tr.d030.prokeimenon1.v1.text = "Οἱ φοβούμενοι τὸν Κύριον, αἰνέσατε αὐτόν."
psTE.tr.d030.prokeimenon1.v2.info = ps_gr_GR_cog.psa21.v2.number
psTE.tr.d030.prokeimenon1.v2.text = "Ὁ Θεός, ὁ Θεός μου, πρόσχες μοι."
psTE.tr.d030.prokeimenon2.title = "Ψαλμὸς 22."
psTE.tr.d030.prokeimenon2.v1.info = ps_gr_GR_cog.psa22.v4.number
psTE.tr.d030.prokeimenon2.v1.text = "Ἡ ῥάβδος σου καὶ ἡ βακτηρία σου, αὗταί με παρεκάλεσαν."
psTE.tr.d030.prokeimenon2.v2.info = ps_gr_GR_cog.psa22.v1.number
psTE.tr.d030.prokeimenon2.v2.text = "Κύριος ποιμαίνει με καὶ οὐδέν με ὑστερήσει."
psVE.tr.d031.prokeimenon1.title = "Ψαλμὸς 23."
psVE.tr.d031.prokeimenon1.v1.info = ps_gr_GR_cog.psa23.v8.number
psVE.tr.d031.prokeimenon1.v1.text = "Κύριος κραταιὸς καὶ δυνατός."
psVE.tr.d031.prokeimenon1.v2.info = ps_gr_GR_cog.psa23.v1.number
psVE.tr.d031.prokeimenon1.v2.text = "Τοῦ Κυρίου ἡ γῆ καὶ τὸ πλήρωμα αὐτῆς."
psVE.tr.d031.prokeimenon2.title = "Ψαλμὸς 24."
psVE.tr.d031.prokeimenon2.v1.info = ps_gr_GR_cog.psa24.v16.number
psVE.tr.d031.prokeimenon2.v1.text = "Ἐπίβλεψον ἐπʼ ἐμὲ καὶ ἐλέησόν με."
psVE.tr.d031.prokeimenon2.v2.info = ps_gr_GR_cog.psa24.v1.number
psVE.tr.d031.prokeimenon2.v2.text = "Πρὸς σέ, Κύριε, ἦρα τὴν ψυχήν μου."

//Tue 2
psTE.tr.d031.prokeimenon1.title = "Ψαλμὸς 25."
psTE.tr.d031.prokeimenon1.v1.info = ps_gr_GR_cog.psa25.v8.number
psTE.tr.d031.prokeimenon1.v1.text = "Κύριε, ἠγάπησα εὐπρέπειαν Οἴκου σου."
psTE.tr.d031.prokeimenon1.v2.info = ps_gr_GR_cog.psa25.v1.number
psTE.tr.d031.prokeimenon1.v2.text = "Κρίνόν με, Κύριε, ὅτι ἐγὼ ἐν ἀκακίᾳ μου ἐπορεύθην."
psTE.tr.d031.prokeimenon2.title = "Ψαλμὸς 26."
psTE.tr.d031.prokeimenon2.v1.info = ps_gr_GR_cog.psa26.v1.number
psTE.tr.d031.prokeimenon2.v1.text = "Κύριος φωτισμός μου καὶ σωτήρ μου."
psTE.tr.d031.prokeimenon2.v2.info = ps_gr_GR_cog.psa26.v1.number
psTE.tr.d031.prokeimenon2.v2.text = "Κύριος ὑπερασπιστὴς τῆς ζωῆς μου."
psVE.tr.d032.prokeimenon1.title = "Ψαλμὸς 27."
psVE.tr.d032.prokeimenon1.v1.info = ps_gr_GR_cog.psa27.v9.number
psVE.tr.d032.prokeimenon1.v1.text = "Σῶσον, Κύριε, τὸν λαόν σου καὶ εὐλόγησον τὴν κληρονομίαν σου."
psVE.tr.d032.prokeimenon1.v2.info = ps_gr_GR_cog.psa27.v1.number
psVE.tr.d032.prokeimenon1.v2.text = "Πρὸς σέ, Κύριε, κεκράξομαι, ὁ Θεός μου."
psVE.tr.d032.prokeimenon2.title = "Ψαλμὸς 28."
psVE.tr.d032.prokeimenon2.v1.info = ps_gr_GR_cog.psa28.v11.number
psVE.tr.d032.prokeimenon2.v1.text = "Κύριος ἰσχὺν τῷ λαῷ αὐτοῦ δώσει."
psVE.tr.d032.prokeimenon2.v2.info = ps_gr_GR_cog.psa28.v1.number
psVE.tr.d032.prokeimenon2.v2.text = "Ἐνέγκατε τῷ Κυρίῳ, υἱοὶ Θεοῦ."

//Wed 2
psTE.tr.d032.prokeimenon1.title = "Ψαλμὸς 29."
psTE.tr.d032.prokeimenon1.v1.info = ps_gr_GR_cog.psa29.v9.number
psTE.tr.d032.prokeimenon1.v1.text = "Πρὸς σέ, Κύριε, κεκράξομαι, ὁ Θεός μου."
psTE.tr.d032.prokeimenon1.v2.info = ps_gr_GR_cog.psa29.v2.number
psTE.tr.d032.prokeimenon1.v2.text = "Ὑψώσω σε, Κύριε, ὅτι ὑπέλαβές με."
psTE.tr.d032.prokeimenon2.title = "Ψαλμὸς 30."
psTE.tr.d032.prokeimenon2.v1.info = ps_gr_GR_cog.psa30.v2.number
psTE.tr.d032.prokeimenon2.v1.text = "Ἐπὶ σοί, Κύριε, ἤλπισα, μὴ καταισχυνθείην εἰς τὸν αἰῶνα."
psTE.tr.d032.prokeimenon2.v2.info = ps_gr_GR_cog.psa30.v2.number
psTE.tr.d032.prokeimenon2.v2.text = "Ἐν τῇ δικαιοσύνῃ σου ῥῦσαί με καὶ ἐξελοῦ με."
psVE.tr.d033.prokeimenon1.title = "Ψαλμὸς 31."
psVE.tr.d033.prokeimenon1.v1.info = ps_gr_GR_cog.psa31.v11.number
psVE.tr.d033.prokeimenon1.v1.text = "Εὐφράνθητε ἐπὶ Κύριον καὶ ἀγαλλιᾶσθε, δίκαιοι."
psVE.tr.d033.prokeimenon1.v2.info = ps_gr_GR_cog.psa31.v1.number
psVE.tr.d033.prokeimenon1.v2.text = "Μακάριοι ὧν ἀφέθησαν αἱ ἀνομίαι."
psVE.tr.d033.prokeimenon2.title = "Ψαλμὸς 32."
psVE.tr.d033.prokeimenon2.v1.info = ps_gr_GR_cog.psa32.v22.number
psVE.tr.d033.prokeimenon2.v1.text = "Γένοιτο, Κύριε, τὸ ἔλεός σου ἐφʼ ἡμᾶς."
psVE.tr.d033.prokeimenon2.v2.info = ps_gr_GR_cog.psa32.v1.number
psVE.tr.d033.prokeimenon2.v2.text = "Ἀγαλλιᾶσθε δίκαιοι ἐν Κυρίῳ."

//Thu 2
psTE.tr.d033.prokeimenon1.title = "Ψαλμὸς 33."
psTE.tr.d033.prokeimenon1.v1.info = ps_gr_GR_cog.psa33.v5.number
psTE.tr.d033.prokeimenon1.v1.text = "Ἐξεζήτησα τὸν Κύριον, καὶ ἐπήκουσέ μου."
psTE.tr.d033.prokeimenon1.v2.info = ps_gr_GR_cog.psa33.v2.number
psTE.tr.d033.prokeimenon1.v2.text = "Εὐλογήσω τὸν Κύριον ἐν παντὶ καιρῷ."
psTE.tr.d033.prokeimenon2.title = "Ψαλμὸς 34."
psTE.tr.d033.prokeimenon2.v1.info = ps_gr_GR_cog.psa34.v23.number
psTE.tr.d033.prokeimenon2.v1.text = "Ἐξεγέρθητι, Κύριε, καὶ πρόσχες τῇ κρίσει μου."
psTE.tr.d033.prokeimenon2.v2.info = ps_gr_GR_cog.psa34.v1.number
psTE.tr.d033.prokeimenon2.v2.text = "Δίκασον, Κύριε, τοὺς ἀδικοῦντάς με, πολέμησον τοὺς πολεμοῦντάς με."
psVE.tr.d034.prokeimenon1.title = "Ψαλμὸς 35."
psVE.tr.d034.prokeimenon1.v1.info = ps_gr_GR_cog.psa35.v6.number
psVE.tr.d034.prokeimenon1.v1.text = "Κύριε, ἐν τῷ Οὐρανῷ τὸ ἔλεός σου."
psVE.tr.d034.prokeimenon1.v2.info = ps_gr_GR_cog.psa35.v7.number
psVE.tr.d034.prokeimenon1.v2.text = "Ἡ δικαιοσύνη σου ὡς ὄρη Θεοῦ."
psVE.tr.d034.prokeimenon2.title = "Ψαλμὸς 36."
psVE.tr.d034.prokeimenon2.v1.info = ps_gr_GR_cog.psa36.v34.number
psVE.tr.d034.prokeimenon2.v1.text = "Ὑπόμεινον τὸν Κύριον καὶ φύλαξον τὴν ὁδὸν αὐτοῦ."
psVE.tr.d034.prokeimenon2.v2.info = ps_gr_GR_cog.psa36.v1.number
psVE.tr.d034.prokeimenon2.v2.text = "Μὴ παραζήλου ἐν πονηρευομένοις."

//Fri 2
psTE.tr.d034.prokeimenon1.title = "Ψαλμὸς 37."
psTE.tr.d034.prokeimenon1.v1.info = ps_gr_GR_cog.psa37.v2.number
psTE.tr.d034.prokeimenon1.v1.text = "Κύριε, μὴ τῷ θυμῷ σου ἐλέγξῃς με, μηδὲ τῇ ὀργῇ σου παιδεύσῃς με."
psTE.tr.d034.prokeimenon1.v2.info = ps_gr_GR_cog.psa37.v3.number
psTE.tr.d034.prokeimenon1.v2.text = "Ὅτι τὰ βέλη σου ἐνεπάγησάν μοι, καὶ ἐπεστήριξας ἐπʼ ἐμὲ τὴν χεῖρά σου."
psTE.tr.d034.prokeimenon2.title = "Ψαλμὸς 38."
psTE.tr.d034.prokeimenon2.v1.info = ps_gr_GR_cog.psa38.v13.number
psTE.tr.d034.prokeimenon2.v1.text = "Εἰσάκουσον τῆς προσευχῆς μου, Κύριε."
psTE.tr.d034.prokeimenon2.v2.info = ps_gr_GR_cog.psa38.v2.number
psTE.tr.d034.prokeimenon2.v2.text = "Εἶπα· φυλάξω τὰς ὁδούς μου."
psVE.tr.d035.prokeimenon1.title = "Ψαλμὸς 39."
psVE.tr.d035.prokeimenon1.v1.info = ps_gr_GR_cog.psa39.v12.number
psVE.tr.d035.prokeimenon1.v1.text = "Τὸ ἔλεός σου, Κύριε, καὶ ἡ ἀλήθειά σου."
psVE.tr.d035.prokeimenon1.v2.info = ps_gr_GR_cog.psa39.v2.number
psVE.tr.d035.prokeimenon1.v2.text = "Ὑπομένων ὑπέμεινα τὸν Κύριον."
psVE.tr.d035.prokeimenon2.title = "Ψαλμὸς 40."
psVE.tr.d035.prokeimenon2.v1.info = ps_gr_GR_cog.psa40.v5.number
psVE.tr.d035.prokeimenon2.v1.text = "Ἐγὼ εἶπα· Κύριε, ἐλέησόν με."
psVE.tr.d035.prokeimenon2.v2.info = ps_gr_GR_cog.psa40.v2.number
psVE.tr.d035.prokeimenon2.v2.text = "Μακάριος ὁ συνιὼν ἐπὶ πτωχόν."

psLI.tr.d035.prokeimenon.title = "Ψαλμὸς 31."
psLI.tr.d035.prokeimenon.v1.info = ps_gr_GR_cog.psa31.v1.number
psLI.tr.d035.prokeimenon.v1.text = "Μακάριοι ὧν ἀφέθησαν αἱ ἀνομίαι καὶ ὧν ἐπεκαλύφθησαν αἱ ἁμαρτίαι."
psLI.tr.d035.prokeimenon.v2.info = ps_gr_GR_cog.psa31.v11.number
psLI.tr.d035.prokeimenon.v2.text = "Εὐφράνθητε ἐπὶ Κύριον καὶ ἀγαλλιᾶσθε, δίκαιοι, καὶ καυχᾶσθε, πάντες οἱ εὐθεῖς τῇ καρδίᾳ."

//Mon 3
psTE.tr.d037.prokeimenon1.title = "Ψαλμὸς 41."
psTE.tr.d037.prokeimenon1.v1.info = ps_gr_GR_cog.psa41.v12.number
psTE.tr.d037.prokeimenon1.v1.text = "Ἔλπισον ἐπὶ τὸν Θεόν, ὅτι ἐξομολογήσομαι αὐτῷ."
psTE.tr.d037.prokeimenon1.v2.info = ps_gr_GR_cog.psa41.v2.number
psTE.tr.d037.prokeimenon1.v2.text = "Ὃν τρόπον ἐπιποθεῖ ἡ ἔλαφος ἐπὶ τὰς πηγὰς τῶν ὑδάτων."
psTE.tr.d037.prokeimenon2.title = "Ψαλμὸς 42."
psTE.tr.d037.prokeimenon2.v1.info = ps_gr_GR_cog.psa42.v5.number
psTE.tr.d037.prokeimenon2.v1.text = "Σωτήριον τοῦ προσώπου μου καὶ ὁ Θεός μου."
psTE.tr.d037.prokeimenon2.v2.info = ps_gr_GR_cog.psa42.v1.number
psTE.tr.d037.prokeimenon2.v2.text = "Κρῖνόν με, ὁ Θεός, καὶ δίκασον τὴν δίκην μου."
psVE.tr.d038.prokeimenon1.title = "Ψαλμὸς 43."
psVE.tr.d038.prokeimenon1.v1.info = ps_gr_GR_cog.psa43.v9.number
psVE.tr.d038.prokeimenon1.v1.text = "Ἐν τῷ Θεῷ ἐπαινεθησόμεθα ὅλην τὴν ἡμέραν."
psVE.tr.d038.prokeimenon1.v2.info = ps_gr_GR_cog.psa43.v2.number
psVE.tr.d038.prokeimenon1.v2.text = "Ὁ Θεός, ἐν τοῖς ὠσὶν ἡμῶν ἠκούσαμεν."
psVE.tr.d038.prokeimenon2.title = "Ψαλμὸς 44."
psVE.tr.d038.prokeimenon2.v1.info = ps_gr_GR_cog.psa44.v18.number
psVE.tr.d038.prokeimenon2.v1.text = "Μνησθήσονται τοῦ Ὀνόματός σου ἐν πάσῃ γενεᾷ καὶ γενεᾷ."
psVE.tr.d038.prokeimenon2.v2.info = ps_gr_GR_cog.psa44.v2.number
psVE.tr.d038.prokeimenon2.v2.text = "Ἐξηρεύξατο ἡ καρδία μου λόγον ἀγαθόν."

//Tue 3
psTE.tr.d038.prokeimenon1.title = "Ψαλμὸς 45."
psTE.tr.d038.prokeimenon1.v1.info = ps_gr_GR_cog.psa45.v12.number
psTE.tr.d038.prokeimenon1.v1.text = "Κύριος τῶν δυνάμεων μεθʼ ἡμῶν."
psTE.tr.d038.prokeimenon1.v2.info = ps_gr_GR_cog.psa45.v2.number
psTE.tr.d038.prokeimenon1.v2.text = "Ὁ Θεὸς ἡμῶν καταφυγὴ καὶ δύναμις."
psTE.tr.d038.prokeimenon2.title = "Ψαλμὸς 46."
psTE.tr.d038.prokeimenon2.v1.info = ps_gr_GR_cog.psa46.v7.number
psTE.tr.d038.prokeimenon2.v1.text = "Ψάλατε τῷ Θεῷ ἡμῶν, ψάλατε."
psTE.tr.d038.prokeimenon2.v2.info = ps_gr_GR_cog.psa46.v2.number
psTE.tr.d038.prokeimenon2.v2.text = "Πάντα τὰ ἔθνη κροτήσατε χεῖρας."
psVE.tr.d039.prokeimenon1.title = "Ψαλμὸς 47."
psVE.tr.d039.prokeimenon1.v1.info = ps_gr_GR_cog.psa47.v2.number
psVE.tr.d039.prokeimenon1.v1.text = "Μέγας Κύριος καὶ αἰνετὸς σφόδρα."
psVE.tr.d039.prokeimenon1.v2.info = ps_gr_GR_cog.psa47.v2.number
psVE.tr.d039.prokeimenon1.v2.text = "Ἐν πόλει τοῦ Θεοῦ ἡμῶν, ἐν ὄρει ἁγίῳ αὐτοῦ."
psVE.tr.d039.prokeimenon2.title = "Ψαλμὸς 48."
psVE.tr.d039.prokeimenon2.v1.info = ps_gr_GR_cog.psa48.v4.number
psVE.tr.d039.prokeimenon2.v1.text = "Τὸ στόμα μου λαλήσει σοφίαν."
psVE.tr.d039.prokeimenon2.v2.info = ps_gr_GR_cog.psa48.v2.number
psVE.tr.d039.prokeimenon2.v2.text = "Ἀκούσατε ταῦτα, πάντα τὰ ἔθνη."

//Wed 3
psTE.tr.d039.prokeimenon1.title = "Ψαλμὸς 49."
psTE.tr.d039.prokeimenon1.v1.info = ps_gr_GR_cog.psa49.v14.number
psTE.tr.d039.prokeimenon1.v1.text = "Θῦσον τῷ Θεῷ θυσίαν αἰνέσεως."
psTE.tr.d039.prokeimenon1.v2.info = ps_gr_GR_cog.psa49.v1.number
psTE.tr.d039.prokeimenon1.v2.text = "Θεὸς θεῶν Κύριος ἐλάλησε, καὶ ἐκάλεσε τὴν γῆν."
psTE.tr.d039.prokeimenon2.title = "Ψαλμὸς 50."
psTE.tr.d039.prokeimenon2.v1.info = ps_gr_GR_cog.psa50.v3.number
psTE.tr.d039.prokeimenon2.v1.text = "Ἐλέησόν με, ὁ Θεός, κατὰ τὸ μέγα ἔλεός σου."
psTE.tr.d039.prokeimenon2.v2.info = ps_gr_GR_cog.psa50.v12.number
psTE.tr.d039.prokeimenon2.v2.text = "Καρδίαν καθαρὰν κτίσον ἐν ἐμοί, ὁ Θεός."
psVE.tr.d040.prokeimenon1.title = "Ψαλμὸς 51."
psVE.tr.d040.prokeimenon1.v1.info = ps_gr_GR_cog.psa51.v10.number
psVE.tr.d040.prokeimenon1.v1.text = "Ἤλπισα ἐπὶ τὸ ἔλεος τοῦ Θεοῦ εἰς τὸν αἰῶνα."
psVE.tr.d040.prokeimenon1.v2.info = ps_gr_GR_cog.psa51.v3.number
psVE.tr.d040.prokeimenon1.v2.text = "Τί ἐγκαυχᾷ ἐν κακίᾳ ὁ δυνατός;"
psVE.tr.d040.prokeimenon2.title = "Ψαλμὸς 52."
psVE.tr.d040.prokeimenon2.v1.info = ps_gr_GR_cog.psa52.v7.number
psVE.tr.d040.prokeimenon2.v1.text = "Ἐν τῷ ἐπιστρέψαι Κύριον τὴν αἰχμαλωσίαν τοῦ λαοῦ αὐτοῦ."
psVE.tr.d040.prokeimenon2.v2.info = ps_gr_GR_cog.psa52.v2.number
psVE.tr.d040.prokeimenon2.v2.text = "Εἶπεν ἄφρων ἐν καρδίᾳ αὐτοῦ· Οὐκ ἔστι Θεός."

//Thu 3
psTE.tr.d040.prokeimenon1.title = "Ψαλμὸς 53."
psTE.tr.d040.prokeimenon1.v1.info = ps_gr_GR_cog.psa53.v3.number
psTE.tr.d040.prokeimenon1.v1.text = "Ὁ Θεός, ἐν τῷ ὀνόματί σου σῶσόν με."
psTE.tr.d040.prokeimenon1.v2.info = ps_gr_GR_cog.psa53.v4.number
psTE.tr.d040.prokeimenon1.v2.text = "Ὁ Θεός, εἰσάκουσον τῆς προσευχῆς μου."
psTE.tr.d040.prokeimenon2.title = "Ψαλμὸς 54."
psTE.tr.d040.prokeimenon2.v1.info = ps_gr_GR_cog.psa54.v2.number
psTE.tr.d040.prokeimenon2.v1.text = "Ἐνώτισαι, ὁ Θεός, τὴν προσευχήν μου."
psTE.tr.d040.prokeimenon2.v2.info = ps_gr_GR_cog.psa54.v3.number
psTE.tr.d040.prokeimenon2.v2.text = "Πρόσχες μοι καὶ εἰσάκουσόν μου."
psVE.tr.d041.prokeimenon1.title = "Ψαλμὸς 55."
psVE.tr.d041.prokeimenon1.v1.info = ps_gr_GR_cog.psa55.v2.number
psVE.tr.d041.prokeimenon1.v1.text = "Ἐλέησόν με, ὁ Θεός, ὅτι κατεπάτησέ με ἄνθρωπος."
psVE.tr.d041.prokeimenon1.v2.info = ps_gr_GR_cog.psa55.v3.number
psVE.tr.d041.prokeimenon1.v2.text = "Κατεπάτησάν με οἱ ἐχθροί μου ὅλην τὴν ἡμέραν."
psVE.tr.d041.prokeimenon2.title = "Ψαλμὸς 56."
psVE.tr.d041.prokeimenon2.v1.info = ps_gr_GR_cog.psa56.v2.number
psVE.tr.d041.prokeimenon2.v1.text = "Ἐλέησόν με, ὁ Θεός, ἐλέησόν με, ὅτι ἐπὶ σοὶ πέποιθεν ἡ ψυχή μου."
psVE.tr.d041.prokeimenon2.v2.info = ps_gr_GR_cog.psa56.v2.number
psVE.tr.d041.prokeimenon2.v2.text = "Καὶ ἐν τῇ σκιᾷ τῶν πτερύγων σου ἐλπιῶ, ἕως οὗ παρέλθῃ ἡ ἀνομία."

//Fri 3
psTE.tr.d041.prokeimenon1.title = "Ψαλμὸς 57."
psTE.tr.d041.prokeimenon1.v1.info = ps_gr_GR_cog.psa57.v2.number
psTE.tr.d041.prokeimenon1.v1.text = "Εἰ ἀληθῶς ἄρα δικαιοσύνην λαλεῖτε, εὐθείας κρίνετε, υἱοὶ τῶν ἀνθρώπων."
psTE.tr.d041.prokeimenon1.v2.info = ps_gr_GR_cog.psa57.v11.number
psTE.tr.d041.prokeimenon1.v2.text = "Εὐφρανθήσεται δίκαιος, ὅταν ἴδῃ ἐκδίκησιν."
psTE.tr.d041.prokeimenon2.title = "Ψαλμὸς 58."
psTE.tr.d041.prokeimenon2.v1.info = ps_gr_GR_cog.psa58.v2.number
psTE.tr.d041.prokeimenon2.v1.text = "Ἐξελοῦ με ἐκ τῶν ἐχθρῶν μου, ὁ Θεός."
psTE.tr.d041.prokeimenon2.v2.info = ps_gr_GR_cog.psa58.v18.number
psTE.tr.d041.prokeimenon2.v2.text = "Ὁ Θεός, ἀντιλήπτωρ μου εἶ, ὁ Θεός μου, τὸ ἔλεός μου."
psVE.tr.d042.prokeimenon1.title = "Ψαλμὸς 59."
psVE.tr.d042.prokeimenon1.v1.info = ps_gr_GR_cog.psa59.v13.number
psVE.tr.d042.prokeimenon1.v1.text = "Δὸς ἡμῖν βοήθειαν ἐκ θλίψεως, καὶ ματαία σωτηρία ἀνθρώπου."
psVE.tr.d042.prokeimenon1.v2.info = ps_gr_GR_cog.psa59.v3.number
psVE.tr.d042.prokeimenon1.v2.text = "Ὁ Θεός, ἀπώσω ἡμᾶς καὶ καθεῖλες ἡμᾶς."
psVE.tr.d042.prokeimenon2.title = "Ψαλμὸς 60."
psVE.tr.d042.prokeimenon2.v1.info = ps_gr_GR_cog.psa60.v2.number
psVE.tr.d042.prokeimenon2.v1.text = "Εἰσάκουσόν, ὁ Θεός, τῆς δεήσεώς μου· πρόσχες τῇ προσευχῇ μου."
psVE.tr.d042.prokeimenon2.v2.info = ps_gr_GR_cog.psa60.v9.number
psVE.tr.d042.prokeimenon2.v2.text = "Οὕτω ψαλῶ τῷ Ὀνόματί σου εἰς τοὺς αἰῶνας."

psLI.tr.d042.prokeimenon.title = "Ψαλμὸς 31."
psLI.tr.d042.prokeimenon.v1.info = ps_gr_GR_cog.psa31.v1.number
psLI.tr.d042.prokeimenon.v1.text = "Μακάριοι ὧν ἀφέθησαν αἱ ἀνομίαι καὶ ὧν ἐπεκαλύφθησαν αἱ ἁμαρτίαι."
psLI.tr.d042.prokeimenon.v2.info = ps_gr_GR_cog.psa31.v11.number
psLI.tr.d042.prokeimenon.v2.text = "Εὐφράνθητε ἐπὶ Κύριον καὶ ἀγαλλιᾶσθε, δίκαιοι, καὶ καυχᾶσθε, πάντες οἱ εὐθεῖς τῇ καρδίᾳ."

psLI.tr.d043.antiphon1.title = "Ψαλμοὶ 4, 59, 67, 60."
psLI.tr.d043.antiphon1.v1.info = ps_gr_GR_cog.psa4.v7.number
psLI.tr.d043.antiphon1.v1.text = "Ἐσημειώθη ἐφʼ ἡμᾶς τὸ φῶς τοῦ προσώπου σου, Κύριε."
psLI.tr.d043.antiphon1.v2.info = ps_gr_GR_cog.psa59.v6.number
psLI.tr.d043.antiphon1.v2.text = "Ἔδωκας τοῖς φοβουμένοις σε σημείωσιν τοῦ φυγεῖν ἀπὸ προσώπου τόξου."
psLI.tr.d043.antiphon1.v3.info = ps_gr_GR_cog.psa67.v19.number
psLI.tr.d043.antiphon1.v3.text = "Ἀνέβης εἰς ὕψος, ᾐχμαλώτευσας αἰχμαλωσίαν."
psLI.tr.d043.antiphon1.v4.info = ps_gr_GR_cog.psa60.v6.number
psLI.tr.d043.antiphon1.v4.text = "Ἔδωκας κληρονομίαν τοῖς φοβουμένοις τὸ ὄνομά σου, Κύριε."
psLI.tr.d043.antiphon2.title = "Ψαλμοὶ 97, 131, 73, 45."
psLI.tr.d043.antiphon2.v1.info = ps_gr_GR_cog.psa97.v3.number
psLI.tr.d043.antiphon2.v1.text = "Εἴδοσαν πάντα τὰ πέρατα τῆς γῆς τὸ σωτήριον τοῦ Θεοῦ ἡμῶν."
psLI.tr.d043.antiphon2.v2.info = ps_gr_GR_cog.psa131.v7.number
psLI.tr.d043.antiphon2.v2.text = "Προσκυνήσομεν εἰς τὸν τόπον, οὗ ἔστησαν οἱ πόδες αὐτοῦ."
psLI.tr.d043.antiphon2.v3.info = ps_gr_GR_cog.psa73.v12.number
psLI.tr.d043.antiphon2.v3.text = "Ὁ δὲ Θεὸς βασιλεὺς ἡμῶν, πρὸ αἰώνων εἰργάσατο σωτηρίαν ἐν μέσῳ τῆς γῆς."
psLI.tr.d043.antiphon2.v4.info = ps_gr_GR_cog.psa45.v11.number
psLI.tr.d043.antiphon2.v4.text = "Ὑψωθήσομαι ἐν τοῖς ἔθνεσιν· ὑψωθήσομαι ἐν τῇ γῇ."
psLI.tr.d043.antiphon3.title = "Ψαλμοὶ 98, 27."
psLI.tr.d043.antiphon3.v1.info = ps_gr_GR_cog.psa98.v5.number
psLI.tr.d043.antiphon3.v1.text = "Ὑψοῦτε Κύριον τὸν Θεὸν ἡμῶν καὶ προσκυνεῖτε τῷ ὑποποδίῳ τῶν ποδῶν αὐτοῦ, ὅτι ἅγιός ἐστι."
psLI.tr.d043.antiphon3.v2.info = ps_gr_GR_cog.psa27.v9.number
psLI.tr.d043.antiphon3.v2.text = "Σῶσον, ὁ Θεός, τὸν λαόν σου καὶ εὐλόγησον τὴν κληρονομίαν σου."
psLI.tr.d043.antiphon3.v3.info = ps_gr_GR_cog.psa27.v9.number
psLI.tr.d043.antiphon3.v3.text = "Καὶ ποίμανον αὐτοὺς καὶ ἔπαρον αὐτοὺς ἕως τοῦ αἰῶνος."
psLI.tr.d043.prokeimenon.title = "Ψαλμὸς 27."
psLI.tr.d043.prokeimenon.v1.info = ps_gr_GR_cog.psa27.v9.number
psLI.tr.d043.prokeimenon.v1.text = "Σῶσον, Κύριε, τὸν λαόν σου καὶ εὐλόγησον τὴν κληρονομίαν σου."
psLI.tr.d043.prokeimenon.v2.info = ps_gr_GR_cog.psa27.v1.number
psLI.tr.d043.prokeimenon.v2.text = "Πρὸς σέ, Κύριε, κεκράξομαι, ὁ Θεός μου, μὴ παρασιωπήσῃς ἀπʼ ἐμοῦ."
psLI.tr.d043.alleluia.title = "Ψαλμὸς 73."
psLI.tr.d043.alleluia.v1.info = ps_gr_GR_cog.psa73.v2.number
psLI.tr.d043.alleluia.v1.text = "Μνήσθητι τῆς συναγωγῆς σου, ἧς ἐκτήσω ἀπʼ ἀρχῆς."
psLI.tr.d043.alleluia.v2.info = ps_gr_GR_cog.psa73.v12.number
psLI.tr.d043.alleluia.v2.text = "Ὁ δὲ Θεὸς βασιλεὺς ἡμῶν πρὸ αἰώνων, εἰργάσατο σωτηρίαν ἐν μέσῳ τῆς γῆς."
psLI.tr.d043.communion.title = "Ψαλμὸς 4."
psLI.tr.d043.communion.v1.info = ps_gr_GR_cog.psa4.v7.number
psLI.tr.d043.communion.v1.text = "Ἐσημειώθη ἐφʼ ἡμᾶς τὸ φῶς τοῦ προσώπου σου, Κύριε."

//Mon 4
psTE.tr.d044.prokeimenon1.title = "Ψαλμὸς 61."
psTE.tr.d044.prokeimenon1.v1.info = ps_gr_GR_cog.psa61.v8.number
psTE.tr.d044.prokeimenon1.v1.text = "Ἐπὶ τῷ Θεῷ τὸ σωτήριόν μου καὶ ἡ δόξα μου."
psTE.tr.d044.prokeimenon1.v2.info = ps_gr_GR_cog.psa61.v2.number
psTE.tr.d044.prokeimenon1.v2.text = "Οὐχὶ τῷ Θεῷ ὑποταγήσεται ἡ ψυχή μου;"
psTE.tr.d044.prokeimenon2.title = "Ψαλμὸς 62."
psTE.tr.d044.prokeimenon2.v1.info = ps_gr_GR_cog.psa62.v5.number
psTE.tr.d044.prokeimenon2.v1.text = "Οὕτως εὐλογήσω σε ἐν τῇ ζωῇ μου."
psTE.tr.d044.prokeimenon2.v2.info = ps_gr_GR_cog.psa62.v2.number
psTE.tr.d044.prokeimenon2.v2.text = "Ὁ Θεὸς ὁ Θεός μου, πρὸς σὲ ὀρθρίζω."
psVE.tr.d045.prokeimenon1.title = "Ψαλμὸς 63."
psVE.tr.d045.prokeimenon1.v1.info = ps_gr_GR_cog.psa63.v2.number
psVE.tr.d045.prokeimenon1.v1.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου, ἐν τῷ δέεσθαί με πρὸς σέ."
psVE.tr.d045.prokeimenon1.v2.info = ps_gr_GR_cog.psa63.v2.number
psVE.tr.d045.prokeimenon1.v2.text = "Ἀπὸ φόβου ἐχθροῦ ἐξελοῦ τὴν ψυχήν μου."
psVE.tr.d045.prokeimenon2.title = "Ψαλμὸς 64."
psVE.tr.d045.prokeimenon2.v1.info = ps_gr_GR_cog.psa64.v6.number
psVE.tr.d045.prokeimenon2.v1.text = "Ἐπάκουσον ἡμῶν, ὁ Θεός, ἡ ἐλπὶς πάντων τῶν περάτων τῆς γῆς."
psVE.tr.d045.prokeimenon2.v2.info = ps_gr_GR_cog.psa64.v2.number
psVE.tr.d045.prokeimenon2.v2.text = "Σοὶ πρέπει ὕμνος, ὁ Θεός, ἐν Σιών."

//Tue 4
psTE.tr.d045.prokeimenon1.title = "Ψαλμὸς 65."
psTE.tr.d045.prokeimenon1.v1.info = ps_gr_GR_cog.psa65.v1.number
psTE.tr.d045.prokeimenon1.v1.text = "Ἀλαλάξατε τῷ Κυρίῳ πᾶσα ἡ γῆ."
psTE.tr.d045.prokeimenon1.v2.info = ps_gr_GR_cog.psa65.v8.number
psTE.tr.d045.prokeimenon1.v2.text = "Εὐλογεῖτε ἔθνη τὸν Θεὸν ἡμῶν, καὶ ἀκουτίσασθε τὴν φωνὴν τῆς αἰνέσεως αὐτοῦ."
psTE.tr.d045.prokeimenon2.title = "Ψαλμὸς 66."
psTE.tr.d045.prokeimenon2.v1.info = ps_gr_GR_cog.psa66.v7.number
psTE.tr.d045.prokeimenon2.v1.text = " Εὐλογήσαι ἡμᾶς ὁ Θεός, ὁ Θεὸς ἡμῶν."
psTE.tr.d045.prokeimenon2.v2.info = ps_gr_GR_cog.psa66.v2.number
psTE.tr.d045.prokeimenon2.v2.text = "Ὁ Θεὸς οἰκτειρήσαι ἡμᾶς καὶ εὐλογήσαι ἡμᾶς."
psVE.tr.d046.prokeimenon1.title = "Ψαλμὸς 67."
psVE.tr.d046.prokeimenon1.v1.info = ps_gr_GR_cog.psa67.v5.number
psVE.tr.d046.prokeimenon1.v1.text = "ᾌσατε τῷ Θεῷ, ψάλατε τῷ Ὀνόματι αὐτοῦ."
psVE.tr.d046.prokeimenon1.v2.info = ps_gr_GR_cog.psa67.v5.number
psVE.tr.d046.prokeimenon1.v2.text = "Ὁδοποιήσατε τῷ ἐπιβεβηκότι ἐπὶ δυσμῶν, Κύριος ὄνομα αὐτῷ."
psVE.tr.d046.prokeimenon2.title = "Ψαλμὸς 68."
psVE.tr.d046.prokeimenon2.v1.info = ps_gr_GR_cog.psa68.v30.number
psVE.tr.d046.prokeimenon2.v1.text = "Ἡ σωτηρία σου, ὁ Θεός, ἀντιλάβοιτό μου."
psVE.tr.d046.prokeimenon2.v2.info = ps_gr_GR_cog.psa68.v33.number
psVE.tr.d046.prokeimenon2.v2.text = "Ἰδέτωσαν πτωχοί, καὶ εὐφρανθήτωσαν."

//Wed 4
psTE.tr.d046.prokeimenon1.title = "Ψαλμὸς 69."
psTE.tr.d046.prokeimenon1.v1.info = ps_gr_GR_cog.psa69.v5.number
psTE.tr.d046.prokeimenon1.v1.text = "Ἀγαλλιάσθωσαν καὶ εὐφρανθήτωσαν ἐπὶ σοὶ πάντες οἱ ζητοῦντές σε, ὁ Θεός."
psTE.tr.d046.prokeimenon1.v2.info = ps_gr_GR_cog.psa69.v2.number
psTE.tr.d046.prokeimenon1.v2.text = "Ὁ Θεός, εἰς τὴν βοήθειάν μου πρόσχες."
psTE.tr.d046.prokeimenon2.title = "Ψαλμὸς 70."
psTE.tr.d046.prokeimenon2.v1.info = ps_gr_GR_cog.psa70.v1.number
psTE.tr.d046.prokeimenon2.v1.text = "Ἐπὶ σοί, Κύριε, ἤλπισα, μὴ καταισχυνθείην εἰς τὸν αἰῶνα."
psTE.tr.d046.prokeimenon2.v2.info = ps_gr_GR_cog.psa70.v2.number
psTE.tr.d046.prokeimenon2.v2.text = "Ἐν τῇ δικαιοσύνῃ σου ῥῦσαί με καὶ ἐξελοῦ με."
psVE.tr.d047.prokeimenon1.title = "Ψαλμὸς 71."
psVE.tr.d047.prokeimenon1.v1.info = ps_gr_GR_cog.psa71.v18.number
psVE.tr.d047.prokeimenon1.v1.text = "Εὐλογητὸς Κύριος, ὁ Θεὸς τοῦ Ἰσραήλ, ὁ ποιῶν θαυμάσια μόνος."
psVE.tr.d047.prokeimenon1.v2.info = ps_gr_GR_cog.psa71.v1.number
psVE.tr.d047.prokeimenon1.v2.text = "Ὁ Θεός, τὸ κρίμα σου τῷ Βασιλεῖ δός."
psVE.tr.d047.prokeimenon2.title = "Ψαλμὸς 72."
psVE.tr.d047.prokeimenon2.v1.info = ps_gr_GR_cog.psa72.v28.number
psVE.tr.d047.prokeimenon2.v1.text = "Ἐμοὶ δὲ τὸ προσκολλᾶσθαι τῷ Θεῷ ἀγαθόν ἐστι."
psVE.tr.d047.prokeimenon2.v2.info = ps_gr_GR_cog.psa72.v1.number
psVE.tr.d047.prokeimenon2.v2.text = "Ὡς ἀγαθὸς ὁ Θεὸς τῷ Ἰσραήλ, τοῖς εὐθέσι τῇ καρδίᾳ!"

//Thu 4
psTE.tr.d047.prokeimenon1.title = "Ψαλμὸς 73."
psTE.tr.d047.prokeimenon1.v1.info = ps_gr_GR_cog.psa73.v12.number
psTE.tr.d047.prokeimenon1.v1.text = "Ὁ δὲ Θεὸς Βασιλεὺς ἡμῶν πρὸ αἰώνων εἰργάσατο σωτηρίαν ἐν μέσῳ τῆς γῆς."
psTE.tr.d047.prokeimenon1.v2.info = ps_gr_GR_cog.psa73.v1.number
psTE.tr.d047.prokeimenon1.v2.text = "Ἰνατί, ὁ Θεός, ἀπώσω εἰς τέλος;"
psTE.tr.d047.prokeimenon2.title = "Ψαλμὸς 74."
psTE.tr.d047.prokeimenon2.v1.info = ps_gr_GR_cog.psa74.v10.number
psTE.tr.d047.prokeimenon2.v1.text = "Ἐγὼ δὲ ἀγαλλιάσομαι εἰς τὸν αἰῶνα."
psTE.tr.d047.prokeimenon2.v2.info = ps_gr_GR_cog.psa74.v2.number
psTE.tr.d047.prokeimenon2.v2.text = "Ἐξομολογησόμεθά σοι, ὁ Θεός."
psVE.tr.d048.prokeimenon1.title = "Ψαλμὸς 75."
psVE.tr.d048.prokeimenon1.v1.info = ps_gr_GR_cog.psa75.v12.number
psVE.tr.d048.prokeimenon1.v1.text = "Εὔξασθε καὶ ἀπόδοτε Κυρίῳ τῷ Θεῷ ἡμῶν."
psVE.tr.d048.prokeimenon1.v2.info = ps_gr_GR_cog.psa75.v2.number
psVE.tr.d048.prokeimenon1.v2.text = "Γνωστὸς ἐν τῇ Ἰουδαίᾳ ὁ Θεός."
psVE.tr.d048.prokeimenon2.title = "Ψαλμὸς 76."
psVE.tr.d048.prokeimenon2.v1.info = ps_gr_GR_cog.psa76.v2.number
psVE.tr.d048.prokeimenon2.v1.text = "Φωνῇ μου πρὸς Κύριον ἐκέκραξα."
psVE.tr.d048.prokeimenon2.v2.info = ps_gr_GR_cog.psa76.v3.number
psVE.tr.d048.prokeimenon2.v2.text = "Ἐν ἡμέρᾳ θλίψεώς μου τὸν Θεὸν ἐξεζήτησα."

//Fri 4
psTE.tr.d048.prokeimenon1.title = "Ψαλμὸς 77."
psTE.tr.d048.prokeimenon1.v1.info = ps_gr_GR_cog.psa77.v38.number
psTE.tr.d048.prokeimenon1.v1.text = "Αὐτός ἐστιν οἰκτίρμων, καὶ ἱλάσκεται ταῖς ἁμαρτίαις ἡμῶν."
psTE.tr.d048.prokeimenon1.v2.info = ps_gr_GR_cog.psa77.v1.number
psTE.tr.d048.prokeimenon1.v2.text = "Προσέχετε, λαός μου, τῷ νόμῳ μου."
psTE.tr.d048.prokeimenon2.title = "Ψαλμὸς 78."
psTE.tr.d048.prokeimenon2.v1.info = ps_gr_GR_cog.psa78.v9.number
psTE.tr.d048.prokeimenon2.v1.text = "Βοήθησον ἡμῖν, ὁ Θεός, ὁ σωτὴρ ἡμῶν."
psTE.tr.d048.prokeimenon2.v2.info = ps_gr_GR_cog.psa78.v1.number
psTE.tr.d048.prokeimenon2.v2.text = "Ὁ Θεός, ἤλθοσαν ἔθνη εἰς τὴν κληρονομίαν σου."
psVE.tr.d049.prokeimenon1.title = "Ψαλμὸς 79."
psVE.tr.d049.prokeimenon1.v1.info = ps_gr_GR_cog.psa79.v2.number
psVE.tr.d049.prokeimenon1.v1.text = "Ὁ καθήμενος ἐπὶ τῶν Χερουβίμ, ἐμφάνηθι."
psVE.tr.d049.prokeimenon1.v2.info = ps_gr_GR_cog.psa79.v2.number
psVE.tr.d049.prokeimenon1.v2.text = "Ὁ ποιμαίνων τὸν Ἰσραήλ, πρόσχες."
psVE.tr.d049.prokeimenon2.title = "Ψαλμὸς 80."
psVE.tr.d049.prokeimenon2.v1.info = ps_gr_GR_cog.psa80.v2.number
psVE.tr.d049.prokeimenon2.v1.text = "Ἀγαλλιᾶσθε τῷ Θεῷ τῷ βοηθῷ ἡμῶν."
psVE.tr.d049.prokeimenon2.v2.info = ps_gr_GR_cog.psa80.v3.number
psVE.tr.d049.prokeimenon2.v2.text = "Λάβετε ψαλμόν, καὶ δότε τύμπανον."

psLI.tr.d049.prokeimenon.title = "Ψαλμοὶ 24, 27."
psLI.tr.d049.prokeimenon.v1.info = ps_gr_GR_cog.psa24.v13.number
psLI.tr.d049.prokeimenon.v1.text = "Αἱ ψυχαὶ αὐτῶν ἐν ἀγαθοῖς αὐλισθήσονται."
psLI.tr.d049.prokeimenon.v2.info = ps_gr_GR_cog.psa27.v1.number
psLI.tr.d049.prokeimenon.v2.text = "Πρὸς σέ, Κύριε, κεκράξομαι, ὁ Θεός μου."

//Mon 5
psTE.tr.d051.prokeimenon1.title = "Ψαλμὸς 83."
psTE.tr.d051.prokeimenon1.v1.info = ps_gr_GR_cog.psa83.v4.number
psTE.tr.d051.prokeimenon1.v1.text = "Τὰ θυσιαστήριά σου, Κύριε τῶν δυνάμεων, ὁ Βασιλεύς μου καὶ ὁ Θεός μου."
psTE.tr.d051.prokeimenon1.v2.info = ps_gr_GR_cog.psa83.v2.number
psTE.tr.d051.prokeimenon1.v2.text = "Ὡς ἀγαπητὰ τὰ σκηνώματά σου, Κύριε τῶν δυνάμεων!"
psTE.tr.d051.prokeimenon2.title = "Ψαλμὸς 84."
psTE.tr.d051.prokeimenon2.v1.info = ps_gr_GR_cog.psa84.v8.number
psTE.tr.d051.prokeimenon2.v1.text = "Δεῖξον ἡμῖν, Κύριε, τὸ ἔλεός σου."
psTE.tr.d051.prokeimenon2.v2.info = ps_gr_GR_cog.psa84.v2.number
psTE.tr.d051.prokeimenon2.v2.text = "Εὐδόκησας, Κύριε, τὴν γῆν σου."
psVE.tr.d052.prokeimenon1.title = "Ψαλμὸς 85."
psVE.tr.d052.prokeimenon1.v1.info = ps_gr_GR_cog.psa85.v11.number
psVE.tr.d052.prokeimenon1.v1.text = "Ὁδήγησόν με, Κύριε, ἐν τῇ ὁδῷ σου, καὶ πορεύσομαι ἐν τῇ ἀληθείᾳ σου."
psVE.tr.d052.prokeimenon1.v2.info = ps_gr_GR_cog.psa85.v1.number
psVE.tr.d052.prokeimenon1.v2.text = "Κλῖνον, Κύριε, τὸ οὖς σου καὶ ἐπάκουσόν μου."
psVE.tr.d052.prokeimenon2.title = "Ψαλμὸς 86."
psVE.tr.d052.prokeimenon2.v1.info = ps_gr_GR_cog.psa86.v2.number
psVE.tr.d052.prokeimenon2.v1.text = "Ἀγαπᾷ Κύριος τὰς πύλας Σιὼν ὑπὲρ πάντα τὰ σκηνώματα Ἰακώβ."
psVE.tr.d052.prokeimenon2.v2.info = ps_gr_GR_cog.psa86.v1.number
psVE.tr.d052.prokeimenon2.v2.text = "Οἱ θεμέλιοι αὐτοῦ ἐν τοῖς ὄρεσι τοῖς ἁγίοις."

//Tue 5
psTE.tr.d052.prokeimenon1.title = "Ψαλμὸς 87."
psTE.tr.d052.prokeimenon1.v1.info = ps_gr_GR_cog.psa87.v16.number
psTE.tr.d052.prokeimenon1.v1.text = "Πτωχός εἰμι ἐγὼ καὶ ἐν κόποις ἐκ νεότητός μου."
psTE.tr.d052.prokeimenon1.v2.info = ps_gr_GR_cog.psa87.v2.number
psTE.tr.d052.prokeimenon1.v2.text = "Κύριε ὁ Θεὸς τῆς σωτηρίας μου, ἡμέρας ἐκέκραξα."
psTE.tr.d052.prokeimenon2.title = "Ψαλμὸς 88."
psTE.tr.d052.prokeimenon2.v1.info = ps_gr_GR_cog.psa88.v6.number
psTE.tr.d052.prokeimenon2.v1.text = "Ἐξομολογήσονται οἱ Οὐρανοὶ τὰ θαυμάσιά σου."
psTE.tr.d052.prokeimenon2.v2.info = ps_gr_GR_cog.psa88.v2.number
psTE.tr.d052.prokeimenon2.v2.text = "Τὰ ἐλέη σου, Κύριε, εἰς τὸν αἰῶνα ᾄσομαι."
psVE.tr.d053.prokeimenon1.title = "Ψαλμὸς 89."
psVE.tr.d053.prokeimenon1.v1.info = ps_gr_GR_cog.psa89.v1.number
psVE.tr.d053.prokeimenon1.v1.text = "Κύριε, καταφυγὴ ἐγενήθης ἡμῖν."
psVE.tr.d053.prokeimenon1.v2.info = ps_gr_GR_cog.psa89.v17.number
psVE.tr.d053.prokeimenon1.v2.text = "Καὶ ἔστω ἡ λαμπρότης Κυρίου τοῦ Θεοῦ ἡμῶν ἐφʼ ἡμᾶς."
psVE.tr.d053.prokeimenon2.title = "Ψαλμὸς 90."
psVE.tr.d053.prokeimenon2.v1.info = ps_gr_GR_cog.psa90.v1.number
psVE.tr.d053.prokeimenon2.v1.text = "Ὁ κατοικῶν ἐν βοηθείᾳ τοῦ Ὑψίστου."
psVE.tr.d053.prokeimenon2.v2.info = ps_gr_GR_cog.psa90.v2.number
psVE.tr.d053.prokeimenon2.v2.text = "Ἐρεῖ τῷ Κυρίῳ· Ἀντιλήπτωρ μου εἶ."

//Wed 5
psTE.tr.d053.prokeimenon1.title = "Ψαλμὸς 91."
psTE.tr.d053.prokeimenon1.v1.info = ps_gr_GR_cog.psa91.v2.number
psTE.tr.d053.prokeimenon1.v1.text = "Ἀγαθὸν τὸ ἐξομολογεῖσθαι τῷ Κυρίῳ καὶ ψάλλειν τῷ Ὀνόματί σου, Ὕψιστε."
psTE.tr.d053.prokeimenon1.v2.info = ps_gr_GR_cog.psa91.v3.number
psTE.tr.d053.prokeimenon1.v2.text = "Τοῦ ἀναγγέλλειν τῷ πρωΐ τὸ ἔλεός σου καὶ τὴν ἀλήθειάν σου καθʼ ἑκάστην."
psTE.tr.d053.prokeimenon2.title = "Ψαλμὸς 92."
psTE.tr.d053.prokeimenon2.v1.info = ps_gr_GR_cog.psa92.v1.number
psTE.tr.d053.prokeimenon2.v1.text = "Ὁ Κύριος ἐβασίλευσεν, εὐπρέπειαν ἐνεδύσατο."
psTE.tr.d053.prokeimenon2.v2.info = ps_gr_GR_cog.psa92.v1.number
psTE.tr.d053.prokeimenon2.v2.text = "Ἐνεδύσατο Κύριος δύναμιν καὶ περιεζώσατο."
psVE.tr.d054.prokeimenon1.title = "Ψαλμὸς 93."
psVE.tr.d054.prokeimenon1.v1.info = ps_gr_GR_cog.psa93.v1.number
psVE.tr.d054.prokeimenon1.v1.text = "Θεὸς ἐκδικήσεων Κύριος, Θεὸς ἐκδικήσεων."
psVE.tr.d054.prokeimenon1.v2.info = ps_gr_GR_cog.psa93.v2.number
psVE.tr.d054.prokeimenon1.v2.text = "Ὑψώθητι ὁ κρίνων τὴν γῆν."
psVE.tr.d054.prokeimenon2.title = "Ψαλμὸς 95."
psVE.tr.d054.prokeimenon2.v1.info = ps_gr_GR_cog.psa95.v1.number
psVE.tr.d054.prokeimenon2.v1.text = "Ἄσατε τῷ Κυρίῳ ᾆσμα καινόν."
psVE.tr.d054.prokeimenon2.v2.info = ps_gr_GR_cog.psa95.v2.number
psVE.tr.d054.prokeimenon2.v2.text = "ᾌσατε τῷ Κυρίῳ, εὐλογήσατε τὸ Ὄνομα αὐτοῦ."

//Thu 5
psTE.tr.d054.prokeimenon1.title = "Ψαλμὸς 96."
psTE.tr.d054.prokeimenon1.v1.info = ps_gr_GR_cog.psa96.v1.number
psTE.tr.d054.prokeimenon1.v1.text = "Ὁ Κύριος ἐβασίλευσεν, ἀγαλλιάσθω ἡ γῆ."
psTE.tr.d054.prokeimenon1.v2.info = ps_gr_GR_cog.psa96.v7.number
psTE.tr.d054.prokeimenon1.v2.text = "Προσκυνήσατε αὐτῷ, πάντες οἱ Ἄγγελοι αὐτοῦ."
psTE.tr.d054.prokeimenon2.title = "Ψαλμὸς 97."
psTE.tr.d054.prokeimenon2.v1.info = ps_gr_GR_cog.psa97.v1.number
psTE.tr.d054.prokeimenon2.v1.text = "Ἄσατε τῷ Κυρίῳ ᾆσμα καινόν."
psTE.tr.d054.prokeimenon2.v2.info = ps_gr_GR_cog.psa97.v4.number
psTE.tr.d054.prokeimenon2.v2.text = "Ἀλαλάξατε τῷ Θεῷ, πᾶσα ἡ γῆ."
psVE.tr.d055.prokeimenon1.title = "Ψαλμὸς 98."
psVE.tr.d055.prokeimenon1.v1.info = ps_gr_GR_cog.psa98.v5.number
psVE.tr.d055.prokeimenon1.v1.text = "Ὑψοῦτε Κύριον τὸν Θεὸν ἡμῶν, καὶ προσκυνεῖτε τῷ ὑποποδίῳ τῶν ποδῶν αὐτοῦ, ὅτι ἅγιός ἐστι."
psVE.tr.d055.prokeimenon1.v2.info = ps_gr_GR_cog.psa98.v1.number
psVE.tr.d055.prokeimenon1.v2.text = "Ὁ Κύριος ἐβασίλευσεν, ὀργιζέσθωσαν λαοί."
psVE.tr.d055.prokeimenon2.title = "Ψαλμὸς 99."
psVE.tr.d055.prokeimenon2.v1.info = ps_gr_GR_cog.psa99.v1.number
psVE.tr.d055.prokeimenon2.v1.text = "Ἀλαλάξατε τῷ Θεῷ, πᾶσα ἡ γῆ."
psVE.tr.d055.prokeimenon2.v2.info = ps_gr_GR_cog.psa99.v2.number
psVE.tr.d055.prokeimenon2.v2.text = "Δουλεύσατε τῷ Κυρίῳ ἐν εὐφροσύνῃ."

//Fri 5
psTE.tr.d055.prokeimenon1.title = "Ψαλμὸς 100."
psTE.tr.d055.prokeimenon1.v1.info = ps_gr_GR_cog.psa100.v1.number
psTE.tr.d055.prokeimenon1.v1.text = "Ἔλεος καὶ κρίσιν ᾄσομαί σοι, Κύριε."
psTE.tr.d055.prokeimenon1.v2.info = ps_gr_GR_cog.psa100.v2.number
psTE.tr.d055.prokeimenon1.v2.text = "Ψαλῶ καὶ συνήσω ἐν ὁδῷ ἀμώμῳ."
psTE.tr.d055.prokeimenon2.title = "Ψαλμὸς 101."
psTE.tr.d055.prokeimenon2.v1.info = ps_gr_GR_cog.psa101.v2.number
psTE.tr.d055.prokeimenon2.v1.text = "Κύριε, εἰσάκουσον τῆς προσευχῆς μου, καὶ ἡ κραυγή μου πρὸς σὲ ἐλθέτω."
psTE.tr.d055.prokeimenon2.v2.info = ps_gr_GR_cog.psa101.v3.number
psTE.tr.d055.prokeimenon2.v2.text = "Μὴ ἀποστρέψῃς τὸ πρόσωπόν σου ἀπʼ ἐμοῦ."
psVE.tr.d056.prokeimenon1.title = "Ψαλμὸς 102."
psVE.tr.d056.prokeimenon1.v1.info = ps_gr_GR_cog.psa102.v8.number
psVE.tr.d056.prokeimenon1.v1.text = "Οἰκτίρμων καὶ ἐλεήμων ὁ Κύριος."
psVE.tr.d056.prokeimenon1.v2.info = ps_gr_GR_cog.psa102.v2.number
psVE.tr.d056.prokeimenon1.v2.text = "Εὐλόγει, ἡ ψυχή μου, τὸν Κύριον."
psVE.tr.d056.prokeimenon2.title = "Ψαλμὸς 103."
psVE.tr.d056.prokeimenon2.v1.info = ps_gr_GR_cog.psa103.v24.number
psVE.tr.d056.prokeimenon2.v1.text = "Ὡς ἐμεγαλύνθη τὰ ἔργα σου, Κύριε!"
psVE.tr.d056.prokeimenon2.v2.info = ps_gr_GR_cog.psa103.v1.number
psVE.tr.d056.prokeimenon2.v2.text = "Εὐλόγει, ἡ ψυχή μου, τὸν Κύριον."

psLI.tr.d056.antiphon3.title = "Ψαλμὸς 44."
psLI.tr.d056.antiphon3.v1.info = ps_gr_GR_cog.psa44.v11.number
psLI.tr.d056.antiphon3.v1.text = "Ἄκουσον, θύγατερ, καὶ ἴδε καὶ κλῖνον τὸ οὖς σου καὶ ἐπιλάθου τοῦ λαοῦ σου καὶ τοῦ οἴκου τοῦ πατρός σου."
psLI.tr.d056.antiphon3.v2.info = ps_gr_GR_cog.psa44.v13.number
psLI.tr.d056.antiphon3.v2.text = "Τὸ πρόσωπόν σου λιτανεύσουσιν οἱ πλούσιοι τοῦ λαοῦ."
psLI.tr.d056.antiphon3.v3.info = ps_gr_GR_cog.psa44.v18.number
psLI.tr.d056.antiphon3.v3.text = "Μνησθήσομαι τοῦ ὀνόματός σου ἐν πάσῃ γενεᾷ καὶ γενεᾷ."
psLI.tr.d056.prokeimenon.title = "ᾨδὴ Ἐνάτη."
psLI.tr.d056.prokeimenon.v1.info = ps_gr_GR_cog.bode9_mag.v1.number
psLI.tr.d056.prokeimenon.v1.text = "Μεγαλύνει ἡ ψυχή μου τὸν Κύριον καὶ ἠγαλλίασε τὸ πνεῦμά μου."
psLI.tr.d056.prokeimenon.v2.info = ps_gr_GR_cog.bode9_mag.v2.number
psLI.tr.d056.prokeimenon.v2.text = "Ὅτι ἐπέβλεψεν ἐπὶ τὴν ταπείνωσιν τῆς δούλης αὐτοῦ."
psLI.tr.d056.alleluia.title = "Ψαλμὸς 131."
psLI.tr.d056.alleluia.v1.info = ps_gr_GR_cog.psa131.v8.number
psLI.tr.d056.alleluia.v1.text = "Ἀνάστηθι, Κύριε, εἰς τὴν ἀνάπαυσίν σου."
psLI.tr.d056.alleluia.v2.info = ps_gr_GR_cog.psa131.v11.number
psLI.tr.d056.alleluia.v2.text = "Ὤμοσε Κύριος τῷ Δαυῒδ ἀλήθειαν καὶ οὐ μὴ ἀθετήσει αὐτήν."
psLI.tr.d056.communion.title = "Ψαλμὸς 115."
psLI.tr.d056.communion.v1.info = ps_gr_GR_cog.psa115.v4.number
psLI.tr.d056.communion.v1.text = "Ποτήριον σωτηρίου λήψομαι, καὶ τὸ ὄνομα Κυρίου ἐπικαλέσομαι."

//Mon 6
psTE.tr.d058.prokeimenon1.title = "Ψαλμὸς 104."
psTE.tr.d058.prokeimenon1.v1.info = ps_gr_GR_cog.psa104.v3.number
psTE.tr.d058.prokeimenon1.v1.text = "Εὐφρανθήτω καρδία ζητούντων τὸν Κύριον."
psTE.tr.d058.prokeimenon1.v2.info = ps_gr_GR_cog.psa104.v1.number
psTE.tr.d058.prokeimenon1.v2.text = "Ἐξομολογεῖσθε τῷ Κυρίῳ, καὶ ἐπικαλεῖσθε τὸ Ὄνομα αὐτοῦ."
psTE.tr.d058.prokeimenon2.title = "Ψαλμὸς 105."
psTE.tr.d058.prokeimenon2.v1.info = ps_gr_GR_cog.psa105.v48.number
psTE.tr.d058.prokeimenon2.v1.text = "Εὐλογητὸς Κύριος, ὁ Θεὸς Ἰσραήλ."
psTE.tr.d058.prokeimenon2.v2.info = ps_gr_GR_cog.psa105.v1.number
psTE.tr.d058.prokeimenon2.v2.text = "Ἐξομολογεῖσθε τῷ Κυρίῳ, ὅτι χρηστός."
psVE.tr.d059.prokeimenon1.title = "Ψαλμὸς 106."
psVE.tr.d059.prokeimenon1.v1.info = ps_gr_GR_cog.psa106.v1.number
psVE.tr.d059.prokeimenon1.v1.text = "Ἐξομολογεῖσθε τῷ Κυρίῳ, ὅτι χρηστός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psVE.tr.d059.prokeimenon1.v2.info = ps_gr_GR_cog.psa106.v2.number
psVE.tr.d059.prokeimenon1.v2.text = "Εἰπάτωσαν οἱ λελυτρωμένοι ὑπὸ Κυρίου."
psVE.tr.d059.prokeimenon2.title = "Ψαλμὸς 107."
psVE.tr.d059.prokeimenon2.v1.info = ps_gr_GR_cog.psa107.v6.number
psVE.tr.d059.prokeimenon2.v1.text = "Ὑψώθητι ἐπὶ τοὺς Οὐρανούς, ὁ Θεός."
psVE.tr.d059.prokeimenon2.v2.info = ps_gr_GR_cog.psa107.v2.number
psVE.tr.d059.prokeimenon2.v2.text = "Ἑτοίμη ἡ καρδία μου, ὁ Θεός."

//Tue 6
psTE.tr.d059.prokeimenon1.title = "Ψαλμὸς 108."
psTE.tr.d059.prokeimenon1.v1.info = ps_gr_GR_cog.psa108.v26.number
psTE.tr.d059.prokeimenon1.v1.text = "Βοήθησόν μοι, Κύριε ὁ Θεός μου."
psTE.tr.d059.prokeimenon1.v2.info = ps_gr_GR_cog.psa108.v1.number
psTE.tr.d059.prokeimenon1.v2.text = "Ὁ Θεός, τὴν αἴνεσίν μου μὴ παρασιωπήσῃς."
psTE.tr.d059.prokeimenon2.title = "Ψαλμὸς 109."
psTE.tr.d059.prokeimenon2.v1.info = ps_gr_GR_cog.psa109.v4.number
psTE.tr.d059.prokeimenon2.v1.text = "Σὺ Ἱερεὺς εἰς τὸν αἰῶνα, κατὰ τὴν τάξιν Μελχισεδέκ."
psTE.tr.d059.prokeimenon2.v2.info = ps_gr_GR_cog.psa109.v1.number
psTE.tr.d059.prokeimenon2.v2.text = "Εἶπεν ὁ Κύριος τῷ Κυρίῳ μου· Κάθου ἐκ δεξιῶν μου."
psVE.tr.d060.prokeimenon1.title = "Ψαλμὸς 110."
psVE.tr.d060.prokeimenon1.v1.info = ps_gr_GR_cog.psa110.v10.number
psVE.tr.d060.prokeimenon1.v1.text = "Ἀρχὴ σοφίας φόβος Κυρίου."
psVE.tr.d060.prokeimenon1.v2.info = ps_gr_GR_cog.psa110.v1.number
psVE.tr.d060.prokeimenon1.v2.text = "Ἐξομολογήσομαί σοι, Κύριε, ἐν ὅλῃ καρδίᾳ μου."
psVE.tr.d060.prokeimenon2.title = "Ψαλμὸς 111."
psVE.tr.d060.prokeimenon2.v1.info = ps_gr_GR_cog.psa111.v4.number
psVE.tr.d060.prokeimenon2.v1.text = "Ἐλεήμων, καὶ οἰκτίρμων, καὶ δίκαιος."
psVE.tr.d060.prokeimenon2.v2.info = ps_gr_GR_cog.psa111.v1.number
psVE.tr.d060.prokeimenon2.v2.text = "Μακάριος ἀνήρ, ὁ φοβούμενος τὸν Κύριον."

//Wed 6
psTE.tr.d060.prokeimenon1.title = "Ψαλμὸς 112."
psTE.tr.d060.prokeimenon1.v1.info = ps_gr_GR_cog.psa112.v1.number
psTE.tr.d060.prokeimenon1.v1.text = "Αἰνεῖτε, παῖδες, Κύριον, αἰνεῖτε τὸ Ὄνομα Κυρίου."
psTE.tr.d060.prokeimenon1.v2.info = ps_gr_GR_cog.psa112.v2.number
psTE.tr.d060.prokeimenon1.v2.text = "Εἴη τὸ ὄνομα Κυρίου εὐλογημένον."
psTE.tr.d060.prokeimenon2.title = "Ψαλμὸς 113."
psTE.tr.d060.prokeimenon2.v1.info = ps_gr_GR_cog.psa113.v23.number
psTE.tr.d060.prokeimenon2.v1.text = "Εὐλογημένοι ὑμεῖς τῷ Κυρίῳ, τῷ ποιήσαντι τὸν Οὐρανὸν καὶ τὴν γῆν."
psTE.tr.d060.prokeimenon2.v2.info = ps_gr_GR_cog.psa113.v1.number
psTE.tr.d060.prokeimenon2.v2.text = "Ἐν ἐξόδῳ Ἰσραὴλ ἐξ Αἰγύπτου."
psVE.tr.d061.prokeimenon1.title = "Ψαλμὸς 114."
psVE.tr.d061.prokeimenon1.v1.info = ps_gr_GR_cog.psa114.v9.number
psVE.tr.d061.prokeimenon1.v1.text = "Εὐαρεστήσω ἐνώπιον Κυρίου, ἐν χώρᾳ ζώντων."
psVE.tr.d061.prokeimenon1.v2.info = ps_gr_GR_cog.psa114.v1.number
psVE.tr.d061.prokeimenon1.v2.text = "Ἠγάπησα, ὅτι εἰσακούσεται Κύριος τῆς φωνῆς τῆς δεήσεώς μου."
psVE.tr.d061.prokeimenon2.title = "Ψαλμὸς 115."
psVE.tr.d061.prokeimenon2.v1.info = ps_gr_GR_cog.psa115.v8.number
psVE.tr.d061.prokeimenon2.v1.text = "Τὰς εὐχάς μου τῷ Κυρίῳ ἀποδώσω, ἐναντίον παντὸς τοῦ λαοῦ αὐτοῦ."
psVE.tr.d061.prokeimenon2.v2.info = ps_gr_GR_cog.psa115.v1.number
psVE.tr.d061.prokeimenon2.v2.text = "Ἐπίστευσα, διὸ ἐλάλησα· ἐγὼ δὲ ἐταπεινώθην σφόδρα."

//Thu 6
psTE.tr.d061.prokeimenon1.title = "Ψαλμὸς 117."
psTE.tr.d061.prokeimenon1.v1.info = ps_gr_GR_cog.psa117.v20.number
psTE.tr.d061.prokeimenon1.v1.text = "Αὕτη ἡ πύλη τοῦ Κυρίου· δίκαιοι εἰσελεύσονται ἐν αὐτῇ."
psTE.tr.d061.prokeimenon1.v2.info = ps_gr_GR_cog.psa117.v1.number
psTE.tr.d061.prokeimenon1.v2.text = "Ἐξομολογεῖσθε τῷ Κυρίῳ, ὅτι ἀγαθός."
psTE.tr.d061.prokeimenon2.title = "Ψαλμὸς 118."
psTE.tr.d061.prokeimenon2.v1.info = ps_gr_GR_cog.psa118.v32.number
psTE.tr.d061.prokeimenon2.v1.text = "Ὁδὸν ἐντολῶν σου ἔδραμον, ὅταν ἐπλάτυνας τὴν καρδίαν μου."
psTE.tr.d061.prokeimenon2.v2.info = ps_gr_GR_cog.psa118.v33.number
psTE.tr.d061.prokeimenon2.v2.text = "Νομοθέτησόν με, Κύριε, τὴν ὁδὸν τῶν δικαιωμάτων σου."
psVE.tr.d062.prokeimenon1.title = "Ψαλμὸς 119."
psVE.tr.d062.prokeimenon1.v1.info = ps_gr_GR_cog.psa119.v1.number
psVE.tr.d062.prokeimenon1.v1.text = "Πρὸς Κύριον ἐν τῷ θλίβεσθαί με, ἐκέκραξα."
psVE.tr.d062.prokeimenon1.v2.info = ps_gr_GR_cog.psa119.v2.number
psVE.tr.d062.prokeimenon1.v2.text = "Κύριε, ῥῦσαι τὴν ψυχήν μου ἀπὸ χειλέων ἀδίκων."
psVE.tr.d062.prokeimenon2.title = "Ψαλμὸς 120."
psVE.tr.d062.prokeimenon2.v1.info = ps_gr_GR_cog.psa120.v7.number
psVE.tr.d062.prokeimenon2.v1.text = "Κύριος φυλάξαι σε ἀπὸ παντὸς κακοῦ, φυλάξοι τὴν ψυχήν σου ὁ Κύριος."
psVE.tr.d062.prokeimenon2.v2.info = ps_gr_GR_cog.psa120.v1.number
psVE.tr.d062.prokeimenon2.v2.text = "Ἦρα τοὺς ὀφθαλμούς μου εἰς τὰ ὄρη."

//Fri 6
psTE.tr.d062.prokeimenon1.title = "Ψαλμὸς 121."
psTE.tr.d062.prokeimenon1.v1.info = ps_gr_GR_cog.psa121.v1.number
psTE.tr.d062.prokeimenon1.v1.text = "Εὐφράνθην ἐπὶ τοῖς εἰρηκόσι μοι. Εἰς οἶκον Κυρίου πορευσόμεθα."
psTE.tr.d062.prokeimenon1.v2.info = ps_gr_GR_cog.psa121.v9.number
psTE.tr.d062.prokeimenon1.v2.text = "Ἕνεκα τοῦ οἴκου Κυρίου τοῦ Θεοῦ ἡμῶν ἐξεζήτησα ἀγαθά σοι."
psTE.tr.d062.prokeimenon2.title = "Ψαλμὸς 122."
psTE.tr.d062.prokeimenon2.v1.info = ps_gr_GR_cog.psa122.v3.number
psTE.tr.d062.prokeimenon2.v1.text = "Ἐλέησον ἡμᾶς, Κύριε, ἐλέησον ἡμᾶς."
psTE.tr.d062.prokeimenon2.v2.info = ps_gr_GR_cog.psa122.v1.number
psTE.tr.d062.prokeimenon2.v2.text = "Πρὸς σὲ ἦρα τοὺς ὀφθαλμούς μου."
psVE.tr.d063.prokeimenon1.title = "Ψαλμὸς 123."
psVE.tr.d063.prokeimenon1.v1.info = ps_gr_GR_cog.psa123.v8.number
psVE.tr.d063.prokeimenon1.v1.text = "Ἡ βοήθεια ἡμῶν ἐν ὀνόματι Κυρίου."
psVE.tr.d063.prokeimenon1.v2.info = ps_gr_GR_cog.psa123.v1.number
psVE.tr.d063.prokeimenon1.v2.text = "Εἰ μὴ ὅτι Κύριος ἦν ἐν ἡμῖν, εἰπάτω δὴ Ἰσραήλ."
psVE.tr.d063.prokeimenon2.title = "Ψαλμὸς 124."
psVE.tr.d063.prokeimenon2.v1.info = ps_gr_GR_cog.psa124.v1.number
psVE.tr.d063.prokeimenon2.v1.text = "Οἱ πεποιθότες ἐπὶ Κύριον ὡς ὄρος Σιών."
psVE.tr.d063.prokeimenon2.v2.info = ps_gr_GR_cog.psa124.v1.number
psVE.tr.d063.prokeimenon2.v2.text = "Οὐ σαλευθήσεται εἰς τὸν αἰῶνα ὁ κατοικῶν Ἰερουσαλήμ."

psLI.tr.d063.antiphon3.title = "Ψαλμοὶ 117, 68."
psLI.tr.d063.antiphon3.v1.info = ps_gr_GR_cog.psa117.v24.number
psLI.tr.d063.antiphon3.v1.text = "Αὕτη ἡ ἡμέρα, ἣν ἐποίησεν ὁ Κύριος· ἀγαλλιασώμεθα καὶ εὐφρανθῶμεν ἐν αὐτῇ."
psLI.tr.d063.antiphon3.v2.info = ps_gr_GR_cog.psa68.v35.number
psLI.tr.d063.antiphon3.v2.text = "Αἰνεσάτωσαν αὐτὸν οἱ οὐρανοὶ καὶ ἡ γῆ, θάλασσα καὶ πάντα τὰ ἕρποντα ἐν αὐτῇ."
psLI.tr.d063.prokeimenon.title = "Ψαλμὸς 26."
psLI.tr.d063.prokeimenon.v1.info = ps_gr_GR_cog.psa26.v1.number
psLI.tr.d063.prokeimenon.v1.text = "Κύριος φωτισμός μου καὶ σωτήρ μου."
psLI.tr.d063.prokeimenon.v2.info = ps_gr_GR_cog.psa26.v1.number
psLI.tr.d063.prokeimenon.v2.text = "Κύριος ὑπερασπιστὴς τῆς ζωῆς μου."
psLI.tr.d063.alleluia.title = "Ψαλμὸς 92."
psLI.tr.d063.alleluia.v1.info = ps_gr_GR_cog.psa92.v1.number
psLI.tr.d063.alleluia.v1.text = "Ὁ Κύριος ἐβασίλευσεν, εὐπρέπειαν ἐνεδύσατο."
psLI.tr.d063.alleluia.v2.info = ps_gr_GR_cog.psa92.v1.number
psLI.tr.d063.alleluia.v2.text = "Καὶ γὰρ ἐστερέωσε τὴν οἰκουμένην, ἥτις οὐ σαλευθήσεται."
psLI.tr.d063.communion.title = "Ψαλμὸς 8."
psLI.tr.d063.communion.v1.info = ps_gr_GR_cog.psa8.v3.number
psLI.tr.d063.communion.v1.text = "Ἐκ στόματος νηπίων καὶ θηλαζόντων κατηρτίσω αἶνον."

psVE.tr.d064.aposverse.v1.info = ps_gr_GR_cog.psa8.v3.number
psVE.tr.d064.aposverse.v1.text = "Ἐκ στόματος νηπίων καὶ θηλαζόντων κατηρτίσω αἶνον."
psVE.tr.d064.aposverse.v2.info = ps_gr_GR_cog.psa8.v2.number
psVE.tr.d064.aposverse.v2.text = "Κύριε ὁ Κύριος ἡμῶν, ὡς θαυμαστὸν τὸ ὄνομά σου ἐν πάσῃ τῇ γῇ."
psMA.tr.d064.prokeimenon.title = "Ψαλμὸς 8."
psMA.tr.d064.prokeimenon.v1.info = ps_gr_GR_cog.psa8.v3.number
psMA.tr.d064.prokeimenon.v1.text = "Ἐκ στόματος νηπίων καὶ θηλαζόντων κατηρτίσω αἶνον."
psMA.tr.d064.prokeimenon.v2.info = ps_gr_GR_cog.psa8.v2.number
psMA.tr.d064.prokeimenon.v2.text = "Κύριε ὁ Κύριος ἡμῶν, ὡς θαυμαστὸν τὸ ὄνομά σου ἐν πάσῃ τῇ γῇ."
psLI.tr.d064.antiphon1.title = "Ψαλμὸς 114."
psLI.tr.d064.antiphon1.v1.info = ps_gr_GR_cog.psa114.v1.number
psLI.tr.d064.antiphon1.v1.text = "Ἠγάπησα, ὅτι εἰσακούσεται Κύριος τῆς φωνῆς τῆς δεήσεώς μου."
psLI.tr.d064.antiphon1.v2.info = ps_gr_GR_cog.psa114.v3.number
psLI.tr.d064.antiphon1.v2.text = "Περιέσχον με ὠδῖνες θανάτου, κίνδυνοι ᾅδου εὕροσάν με."
psLI.tr.d064.antiphon1.v3.info = ps_gr_GR_cog.psa114.v3.number
psLI.tr.d064.antiphon1.v3.text = "Θλῖψιν καὶ ὀδύνην εὗρον, καὶ τὸ ὄνομα Κυρίου ἐπεκαλεσάμην."
psLI.tr.d064.antiphon1.v4.info = ps_gr_GR_cog.psa114.v9.number
psLI.tr.d064.antiphon1.v4.text = "Εὐαρεστήσω ἐνώπιον Κυρίου, ἐν χώρᾳ ζώντων."
psLI.tr.d064.antiphon2.title = "Ψαλμὸς 115."
psLI.tr.d064.antiphon2.v1.info = ps_gr_GR_cog.psa115.v1.number
psLI.tr.d064.antiphon2.v1.text = "Ἐπίστευσα, διὸ ἐλάλησα· ἐγὼ δὲ ἐταπεινώθην σφόδρα."
psLI.tr.d064.antiphon2.v2.info = ps_gr_GR_cog.psa115.v3.number
psLI.tr.d064.antiphon2.v2.text = "Τί ἀνταποδώσω τῷ Κυρίῳ περὶ πάντων, ὧν ἀνταπέδωκέ μοι;"
psLI.tr.d064.antiphon2.v3.info = ps_gr_GR_cog.psa115.v4.number
psLI.tr.d064.antiphon2.v3.text = "Ποτήριον σωτηρίου λήψομαι, καὶ τὸ ὄνομα Κυρίου ἐπικαλέσομαι."
psLI.tr.d064.antiphon2.v4.info = ps_gr_GR_cog.psa115.v8.number
psLI.tr.d064.antiphon2.v4.text = "Τὰς εὐχάς μου τῷ Κυρίῳ ἀποδώσω, ἐναντίον παντὸς τοῦ λαοῦ αὐτοῦ."
psLI.tr.d064.antiphon3.title = "Ψαλμὸς 117."
psLI.tr.d064.antiphon3.v1.info = ps_gr_GR_cog.psa117.v1.number
psLI.tr.d064.antiphon3.v1.text = "Ἐξομολογεῖσθε τῷ Κυρίῳ, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.tr.d064.antiphon3.v2.info = ps_gr_GR_cog.psa117.v2.number
psLI.tr.d064.antiphon3.v2.text = "Εἰπάτω δὴ οἶκος Ἰσραήλ, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.tr.d064.antiphon3.v3.info = ps_gr_GR_cog.psa117.v3.number
psLI.tr.d064.antiphon3.v3.text = "Εἰπάτω δὴ οἶκος Ἀαρών, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.tr.d064.antiphon3.v4.info = ps_gr_GR_cog.psa117.v4.number
psLI.tr.d064.antiphon3.v4.text = "Εἰπάτωσαν δὴ πάντες οἱ φοβούμενοι τὸν Κύριον, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.tr.d064.entrance.title = "Ψαλμὸς 117."
psLI.tr.d064.entrance.v1.info = ps_gr_GR_cog.psa117.v26.number
psLI.tr.d064.entrance.v1.text = "Εὐλογημένος ὁ ἐρχόμενος ἐν ὀνόματι Κυρίου. Θεὸς Κύριος καὶ ἐπέφανεν ἡμῖν."
psLI.tr.d064.prokeimenon.title = "Ψαλμὸς 117."
psLI.tr.d064.prokeimenon.v1.info = ps_gr_GR_cog.psa117.v26.number
psLI.tr.d064.prokeimenon.v1.text = "Εὐλογημένος ὁ ἐρχόμενος ἐν ὀνόματι Κυρίου."
psLI.tr.d064.prokeimenon.v2.info = ps_gr_GR_cog.psa117.v1.number
psLI.tr.d064.prokeimenon.v2.text = "Ἐξομολογεῖσθε τῷ Κυρίῳ, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.tr.d064.alleluia.title = "Ψαλμὸς 97."
psLI.tr.d064.alleluia.v1.info = ps_gr_GR_cog.psa97.v1.number
psLI.tr.d064.alleluia.v1.text = "Ἄσατε τῷ Κυρίῳ ᾆσμα καινόν, ὅτι θαυμαστὰ ἐποίησεν ὁ Κύριος."
psLI.tr.d064.alleluia.v2.info = ps_gr_GR_cog.psa97.v3.number
psLI.tr.d064.alleluia.v2.text = "Εἴδοσαν πάντα τὰ πέρατα τῆς γῆς τὸ σωτήριον τοῦ Θεοῦ ἡμῶν."
psLI.tr.d064.communion.title = "Ψαλμὸς 117."
psLI.tr.d064.communion.v1.info = ps_gr_GR_cog.psa117.v26.number
psLI.tr.d064.communion.v1.text = "Εὐλογημένος ὁ ἐρχόμενος ἐν ὀνόματι Κυρίου."

psVE.tr.d065.aposverse.v1.info = ps_gr_GR_cog.psa97.v1.number
psVE.tr.d065.aposverse.v1.text = "Ἄσατε τῷ Κυρίῳ ᾆσμα καινόν."
psVE.tr.d065.aposverse.v2.info = ps_gr_GR_cog.psa97.v3.number
psVE.tr.d065.aposverse.v2.text = "Εἴδοσαν πάντα τὰ πέρατα τῆς γῆς τὸ σωτήριον τοῦ Θεοῦ ἡμῶν."

//Holy Monday
psTE.tr.d065.prokeimenon1.title = "Ψαλμὸς 125."
psTE.tr.d065.prokeimenon1.v1.info = ps_gr_GR_cog.psa125.v1.number
psTE.tr.d065.prokeimenon1.v1.text = "Ἐν τῷ ἐπιστρέψαι Κύριον τὴν αἰχμαλωσίαν Σιών."
psTE.tr.d065.prokeimenon1.v2.info = ps_gr_GR_cog.psa125.v2.number
psTE.tr.d065.prokeimenon1.v2.text = "Τότε ἐπλήσθη χαρᾶς τὸ στόμα ἡμῶν."
psTE.tr.d065.prokeimenon2.title = "Ψαλμὸς 126."
psTE.tr.d065.prokeimenon2.v1.info = ps_gr_GR_cog.psa126.v1.number
psTE.tr.d065.prokeimenon2.v1.text = "Ἐάν μὴ Κύριος οἰκοδομήσῃ οἶκον, εἰς μάτην ἐκοπίασαν οἱ οἰκοδομοῦντες."
psTE.tr.d065.prokeimenon2.v2.info = ps_gr_GR_cog.psa126.v1.number
psTE.tr.d065.prokeimenon2.v2.text = "Ἐὰν μὴ Κύριος φυλάξῃ πόλιν, εἰς μάτην ἠγρύπνησεν ὁ φυλάσσων."
psVE.tr.d066.prokeimenon1.title = "Ψαλμὸς 127."
psVE.tr.d066.prokeimenon1.v1.info = ps_gr_GR_cog.psa127.v5.number
psVE.tr.d066.prokeimenon1.v1.text = "Εὐλογήσαι σε Κύριος ἐκ Σιών, ὁ ποιήσας τὸν Οὐρανὸν καὶ τὴν γῆν."
psVE.tr.d066.prokeimenon1.v2.info = ps_gr_GR_cog.psa127.v1.number
psVE.tr.d066.prokeimenon1.v2.text = "Μακάριοι πάντες οἱ φοβούμενοι τὸν Κύριον."
psVE.tr.d066.prokeimenon2.title = "Ψαλμὸς 128."
psVE.tr.d066.prokeimenon2.v1.info = ps_gr_GR_cog.psa128.v8.number
psVE.tr.d066.prokeimenon2.v1.text = "Εὐλογήκαμεν ὑμᾶς ἐν ὀνόματι Κυρίου."
psVE.tr.d066.prokeimenon2.v2.info = ps_gr_GR_cog.psa128.v1.number
psVE.tr.d066.prokeimenon2.v2.text = "Πλεονάκις ἐπολέμησάν με ἐκ νεότητός μου."

//Holy Tuesday
psTE.tr.d066.prokeimenon1.title = "Ψαλμὸς 129."
psTE.tr.d066.prokeimenon1.v1.info = ps_gr_GR_cog.psa129.v7.number
psTE.tr.d066.prokeimenon1.v1.text = "Ὅτι παρὰ τῷ Κυρίῳ τὸ ἔλεος, καὶ πολλὴ παρʼ αὐτῷ λύτρωσις."
psTE.tr.d066.prokeimenon1.v2.info = ps_gr_GR_cog.psa129.v1.number
psTE.tr.d066.prokeimenon1.v2.text = "Ἐκ βαθέων ἐκέκραξά σοι Κύριε."
psTE.tr.d066.prokeimenon2.title = "Ψαλμὸς 130."
psTE.tr.d066.prokeimenon2.v1.info = ps_gr_GR_cog.psa130.v3.number
psTE.tr.d066.prokeimenon2.v1.text = "Ἐλπισάτω Ἰσραὴλ ἐπὶ τὸν Κύριον, ἀπὸ τοῦ νῦν καὶ ἕως τοῦ αἰῶνος."
psTE.tr.d066.prokeimenon2.v2.info = ps_gr_GR_cog.psa130.v1.number
psTE.tr.d066.prokeimenon2.v2.text = "Κύριε, οὐχ ὑψώθη ἡ καρδία μου."
psVE.tr.d067.prokeimenon1.title = "Ψαλμὸς 131."
psVE.tr.d067.prokeimenon1.v1.info = ps_gr_GR_cog.psa131.v8.number
psVE.tr.d067.prokeimenon1.v1.text = "Ἀνάστηθι, Κύριε, εἰς τὴν ἀνάπαυσίν σου, σὺ καὶ ἡ κιβωτὸς τοῦ ἁγιάσματός σου."
psVE.tr.d067.prokeimenon1.v2.info = ps_gr_GR_cog.psa131.v1.number
psVE.tr.d067.prokeimenon1.v2.text = "Μνήσθητι, Κύριε, τοῦ Δαυΐδ, καὶ πάσης τῆς πραότητος αὐτοῦ."
psVE.tr.d067.prokeimenon2.title = "Ψαλμὸς 132."
psVE.tr.d067.prokeimenon2.v1.info = ps_gr_GR_cog.psa132.v1.number
psVE.tr.d067.prokeimenon2.v1.text = "Ἰδοὺ δὴ τί καλόν, ἢ τί τερπνόν, ἀλλʼ ἢ τὸ κατοικεῖν ἀδελφοὺς ἐπὶ τὸ αὐτό;"
psVE.tr.d067.prokeimenon2.v2.info = ps_gr_GR_cog.psa132.v2.number
psVE.tr.d067.prokeimenon2.v2.text = "Ὡς μύρον ἐπὶ κεφαλῆς, τὸ καταβαῖνον ἐπὶ πώγωνα."

//Holy Wednesday
psTE.tr.d067.prokeimenon1.title = "Ψαλμὸς 133."
psTE.tr.d067.prokeimenon1.v1.info = ps_gr_GR_cog.psa133.v3.number
psTE.tr.d067.prokeimenon1.v1.text = "Εὐλογήσαι σε Κύριος ἐκ Σιών, ὁ ποιήσας τὸν Οὐρανὸν καὶ τὴν γῆν."
psTE.tr.d067.prokeimenon1.v2.info = ps_gr_GR_cog.psa133.v1.number
psTE.tr.d067.prokeimenon1.v2.text = "Ἰδοὺ δὴ εὐλογεῖτε τὸν Κύριον, πάντες οἱ δοῦλοι Κυρίου."
psTE.tr.d067.prokeimenon2.title = "Ψαλμὸς 134."
psTE.tr.d067.prokeimenon2.v1.info = ps_gr_GR_cog.psa134.v20.number
psTE.tr.d067.prokeimenon2.v1.text = " Οἱ φοβούμενοι τὸν Κύριον, εὐλογήσατε τὸν Κύριον."
psTE.tr.d067.prokeimenon2.v2.info = ps_gr_GR_cog.psa134.v1.number
psTE.tr.d067.prokeimenon2.v2.text = "Αἰνεῖτε τὸ ὄνομα Κυρίου, αἰνεῖτε δοῦλοι Κύριον."
psVE.tr.d068.prokeimenon1.title = "Ψαλμὸς 135."
psVE.tr.d068.prokeimenon1.v1.info = ps_gr_GR_cog.psa135.v26.number
psVE.tr.d068.prokeimenon1.v1.text = "Ἐξομολογεῖσθε τῷ Θεῷ τοῦ Οὐρανοῦ."
psVE.tr.d068.prokeimenon1.v2.info = ps_gr_GR_cog.psa135.v2.number
psVE.tr.d068.prokeimenon1.v2.text = "Ἐξομολογεῖσθε τῷ Θεῷ τῶν θεῶν."
psVE.tr.d068.prokeimenon2.title = "Ψαλμὸς 137."
psVE.tr.d068.prokeimenon2.v1.info = ps_gr_GR_cog.psa137.v8.number
psVE.tr.d068.prokeimenon2.v1.text = "Κύριε, τὸ ἔλεός σου εἰς τὸν αἰῶνα· τὰ ἔργα τῶν χειρῶν σου μὴ παρίδῃς."
psVE.tr.d068.prokeimenon2.v2.info = ps_gr_GR_cog.psa137.v1.number
psVE.tr.d068.prokeimenon2.v2.text = "Ἐξομολογήσομαί σοι, Κύριε, ἐν ὅλῃ καρδίᾳ μου."
psMA.tr.d068.aposverse.v1.info = ps_gr_GR_cog.psa40.v10.number
psMA.tr.d068.aposverse.v1.text = "Ὁ ἐσθίων ἄρτους μου, ἐμεγάλυνεν ἐπʼ ἐμὲ πτερνισμόν."
psMA.tr.d068.aposverse.v2.info = ps_gr_GR_cog.psa40.v7.number
psMA.tr.d068.aposverse.v2.text = "Ἐξεπορεύετο ἔξω 0καὶ ἐλάλει ἐπὶ τὸ αὐτό. Κατʼ ἐμοῦ ἐψιθύριζον πάντες οἱ ἐχθροί μου, κατʼ ἐμοῦ ἐλογίζοντο κακά μοι."
psMA.tr.d068.aposverse.v3.info = ps_gr_GR_cog.psa40.v9.number
psMA.tr.d068.aposverse.v3.text = "Λόγον παράνομον κατέθεντο κατʼ ἐμοῦ· Μὴ ὁ κοιμώμενος οὐχὶ προσθήσει τοῦ ἀναστῆναι;"
psMA.tr.d068.prokeimenon1.title = "Ψαλμὸς 82."
psMA.tr.d068.prokeimenon1.v1.info = ps_gr_GR_cog.psa82.v19.number
psMA.tr.d068.prokeimenon1.v1.text = "Γνώτωσαν ἔθνη, ὅτι ὄνομά σοι Κύριος."
psMA.tr.d068.prokeimenon1.v2.info = ps_gr_GR_cog.psa82.v2.number
psMA.tr.d068.prokeimenon1.v2.text = "Ὁ Θεός, τίς ὁμοιωθήσεταί σοι;"
psMA.tr.d068.prokeimenon2.title = "Ψαλμὸς 75."
psMA.tr.d068.prokeimenon2.v1.info = ps_gr_GR_cog.psa75.v12.number
psMA.tr.d068.prokeimenon2.v1.text = "Εὔξασθε, καὶ ἀπόδοτε Κυρίῳ τῷ Θεῷ ἡμῶν."
psMA.tr.d068.prokeimenon2.v2.info = ps_gr_GR_cog.psa75.v2.number
psMA.tr.d068.prokeimenon2.v2.text = "Γνωστὸς ἐν τῇ Ἰουδαίᾳ ὁ Θεός."

//Holy Thursday
psVE.tr.d069.prokeimenon1.title = "Ψαλμὸς 139."
psVE.tr.d069.prokeimenon1.v1.info = ps_gr_GR_cog.psa139.v2.number
psVE.tr.d069.prokeimenon1.v1.text = "Ἐξελοῦ με, Κύριε, ἐξ ἀνθρώπου πονηροῦ."
psVE.tr.d069.prokeimenon1.v2.info = ps_gr_GR_cog.psa139.v3.number
psVE.tr.d069.prokeimenon1.v2.text = "Οἵτινες ἐλογίσαντο ἀδικίαν ἐν καρδίᾳ."
psVE.tr.d069.prokeimenon2.title = "Ψαλμὸς 58."
psVE.tr.d069.prokeimenon2.v1.info = ps_gr_GR_cog.psa58.v2.number
psVE.tr.d069.prokeimenon2.v1.text = "Ἐξελοῦ με ἐκ τῶν ἐχθρῶν μου, ὁ Θεός."
psVE.tr.d069.prokeimenon2.v2.info = ps_gr_GR_cog.psa58.v3.number
psVE.tr.d069.prokeimenon2.v2.text = "Ῥῦσαί με ἐκ τῶν ἐργαζομένων τὴν ἀνομίαν"

psVL.tr.d069.prokeimenon.title = "Ψαλμὸς 2."
psVL.tr.d069.prokeimenon.v1.info = ps_gr_GR_cog.psa2.v2.number
psVL.tr.d069.prokeimenon.v1.text = "Οἱ ἄρχοντες συνήχθησαν ἐπὶ τὸ αὐτὸ κατὰ τοῦ Κυρίου καὶ κατὰ τοῦ χριστοῦ αὐτοῦ."
psVL.tr.d069.prokeimenon.v2.info = ps_gr_GR_cog.psa2.v1.number
psVL.tr.d069.prokeimenon.v2.text = "Ἰνατί ἐφρύαξαν ἔθνη, καὶ λαοὶ ἐμελέτησαν κενά;"
psVL.tr.d069.alleluia.title = "Ψαλμὸς 40."
psVL.tr.d069.alleluia.v1.info = ps_gr_GR_cog.psa40.v2.number
psVL.tr.d069.alleluia.v1.text = "Μακάριος ὁ συνιὼν ἐπὶ πτωχὸν καὶ πένητα."
psVL.tr.d069.alleluia.v2.info = ps_gr_GR_cog.psa40.v6.number
psVL.tr.d069.alleluia.v2.text = "Οἱ ἐχθροί μου εἶπαν κακά μοι· Πότε ἀποθανεῖται, καὶ ἀπολεῖται τὸ ὄνομα αὐτοῦ;"
psVL.tr.d069.alleluia.v3.info = ps_gr_GR_cog.psa40.v10.number
psVL.tr.d069.alleluia.v3.text = "Ὁ ἐσθίων ἄρτους μου, ἐμεγάλυνεν ἐπʼ ἐμὲ πτερνισμόν."

psMA.tr.d069.prokeimenon.title = "Ψαλμὸς 21."
psMA.tr.d069.prokeimenon.v1.info = ps_gr_GR_cog.psa21.v19.number
psMA.tr.d069.prokeimenon.v1.text = "Διεμερίσαντο τὰ ἱμάτιά μου ἑαυτοῖς καὶ ἐπὶ τὸν ἱματισμόν μου ἔβαλον κλῆρον."
psMA.tr.d069.prokeimenon.v2.info = ps_gr_GR_cog.psa21.v2.number
psMA.tr.d069.prokeimenon.v2.text = "Ὁ Θεός, ὁ Θεός μου, πρόσχες μοι· ἵνα τί ἐγκατέλιπές με;"
psMA.tr.d069.aposverse.v1.info = ps_gr_GR_cog.psa21.v19.number
psMA.tr.d069.aposverse.v1.text = "Διεμερίσαντο τὰ ἱμάτιά μου ἑαυτοῖς καὶ ἐπὶ τὸν ἱματισμόν μου ἔβαλον κλῆρον."
psMA.tr.d069.aposverse.v2.info = ps_gr_GR_cog.psa68.v22.number
psMA.tr.d069.aposverse.v2.text = "Ἔδωκαν εἰς τὸ βρῶμά μου χολὴν καὶ εἰς τὴν δίψαν μου ἐπότισάν με ὄξος."
psMA.tr.d069.aposverse.v3.info = ps_gr_GR_cog.psa73.v12.number
psMA.tr.d069.aposverse.v3.text = "Ὁ δὲ Θεὸς βασιλεὺς ἡμῶν πρὸ αἰώνων, εἰργάσατο σωτηρίαν ἐν μέσῳ τῆς γῆς."

//Holy Friday
psH1.tr.d069.v1.info = ps_gr_GR_cog.psa2.v1.number
psH1.tr.d069.v1.text = "Ἰνατί ἐφρύαξαν ἔθνη, καὶ λαοὶ ἐμελέτησαν κενά;"
psH1.tr.d069.v2.info = ps_gr_GR_cog.psa2.v2.number
psH1.tr.d069.v2.text = "Παρέστησαν οἱ βασιλεῖς τῆς γῆς, καὶ οἱ ἄρχοντες συνήχθησαν ἐπὶ τὸ αὐτό."
psH1.tr.d069.prokeimenon.title = "Ψαλμὸς 40."
psH1.tr.d069.prokeimenon.v1.info = ps_gr_GR_cog.psa40.v7.number
psH1.tr.d069.prokeimenon.v1.text = "Ἐξεπορεύετο ἔξω καὶ ἐλάλει ἐπὶ τὸ αὐτό."
psH1.tr.d069.prokeimenon.v2.info = ps_gr_GR_cog.psa40.v2.number
psH1.tr.d069.prokeimenon.v2.text = "Μακάριος ὁ συνιὼν ἐπὶ πτωχὸν καὶ πένητα."
psH3.tr.d069.v1.info = ps_gr_GR_cog.psa5.v2.number
psH3.tr.d069.v1.text = "Τὰ ῥήματά μου ἐνώτισαι, Κύριε, σύνες τῆς κραυγῆς μου."
psH3.tr.d069.v2.info = ps_gr_GR_cog.psa5.v3.number
psH3.tr.d069.v2.text = "Πρόσχες τῇ φωνῇ τῆς δεήσεώς μου, ὁ Βασιλεύς μου καὶ ὁ Θεός μου."
psH3.tr.d069.prokeimenon.title = "Ψαλμὸς 37."
psH3.tr.d069.prokeimenon.v1.info = ps_gr_GR_cog.psa37.v18.number
psH3.tr.d069.prokeimenon.v1.text = "Ὅτι ἐγὼ εἰς μάστιγας ἕτοιμος."
psH3.tr.d069.prokeimenon.v2.info = ps_gr_GR_cog.psa37.v2.number
psH3.tr.d069.prokeimenon.v2.text = "Κύριε, μὴ τῷ θυμῷ σου ἐλέγξῃς με."
psH6.tr.d069.v1.info = ps_gr_GR_cog.psa68.v22.number
psH6.tr.d069.v1.text = "Ἔδωκαν εἰς τὸ βρῶμά μου χολὴν καὶ εἰς τὴν δίψαν μου ἐπότισάν με ὄξος."
psH6.tr.d069.v2.info = ps_gr_GR_cog.psa68.v2.number
psH6.tr.d069.v2.text = "Σῶσόν με, ὁ Θεός, ὅτι εἰσήλθοσαν ὕδατα ἕως ψυχῆς μου."
psH6.tr.d069.prokeimenon.title = "Ψαλμὸς 8."
psH6.tr.d069.prokeimenon.v1.info = ps_gr_GR_cog.psa8.v2.number
psH6.tr.d069.prokeimenon.v1.text = "Κύριε ὁ Κύριος ἡμῶν, ὡς θαυμαστὸν τὸ Ὄνομά σου ἐν πάσῃ τῇ γῇ."
psH6.tr.d069.prokeimenon.v2.info = ps_gr_GR_cog.psa8.v2.number
psH6.tr.d069.prokeimenon.v2.text = "Ὅτι ἐπήρθη ἡ μεγαλοπρέπειά σου ὑπεράνω τῶν Οὐρανῶν."
psH9.tr.d069.v1.info = ps_gr_GR_cog.psa21.v19.number
psH9.tr.d069.v1.text = "Διεμερίσαντο τὰ ἱμάτιά μου ἑαυτοῖς καὶ ἐπὶ τὸν ἱματισμόν μου ἔβαλον κλῆρον."
psH9.tr.d069.v2.info = ps_gr_GR_cog.psa68.v22.number
psH9.tr.d069.v2.text = "Ἔδωκαν εἰς τὸ βρῶμά μου χολὴν καὶ εἰς τὴν δίψαν μου ἐπότισάν με ὄξος."
psH9.tr.d069.prokeimenon.title = "Ψαλμὸς 13."
psH9.tr.d069.prokeimenon.v1.info = ps_gr_GR_cog.psa13.v1.number
psH9.tr.d069.prokeimenon.v1.text = "Εἶπεν ἄφρων ἐν καρδίᾳ αὐτοῦ· Οὐκ ἔστι Θεός."
psH9.tr.d069.prokeimenon.v2.info = ps_gr_GR_cog.psa13.v1.number
psH9.tr.d069.prokeimenon.v2.text = "Οὐκ ἔστι ποιῶν χρηστότητα, οὐκ ἔστιν ἕως ἑνός."

psVE.tr.d070.prokeimenon1.title = "Ψαλμὸς 21."
psVE.tr.d070.prokeimenon1.v1.info = ps_gr_GR_cog.psa21.v19.number
psVE.tr.d070.prokeimenon1.v1.incipit = "Διεμερίσαντο τὰ ἱμάτιά μου…"
psVE.tr.d070.prokeimenon1.v1.text = "Διεμερίσαντο τὰ ἱμάτιά μου ἑαυτοῖς καὶ ἐπὶ τὸν ἱματισμόν μου ἔβαλον κλῆρον."
psVE.tr.d070.prokeimenon1.v2.info = ps_gr_GR_cog.psa21.v2.number
psVE.tr.d070.prokeimenon1.v2.text = "Ὁ Θεός, ὁ Θεός μου, πρόσχες μοι· ἵνα τί ἐγκατέλιπές με;"
psVE.tr.d070.prokeimenon2.title = "Ψαλμὸς 34."
psVE.tr.d070.prokeimenon2.v1.info = ps_gr_GR_cog.psa34.v1.number
psVE.tr.d070.prokeimenon2.v1.text = "Δίκασον, Κύριε, τοὺς ἀδικοῦντάς με."
psVE.tr.d070.prokeimenon2.v2.info = ps_gr_GR_cog.psa34.v12.number
psVE.tr.d070.prokeimenon2.v2.text = "Ἀνταπεδίδοσάν μοι πονηρὰ ἀντὶ ἀγαθῶν."
psVE.tr.d070.prokeimenon3.title = "Ψαλμὸς 87."
psVE.tr.d070.prokeimenon3.v1.info = ps_gr_GR_cog.psa87.v7.number
psVE.tr.d070.prokeimenon3.v1.text = "Ἔθεντό με ἐν λάκκῳ κατωτάτῳ, ἐν σκοτεινοῖς καὶ ἐν σκιᾷ θανάτου."
psVE.tr.d070.prokeimenon3.v2.info = ps_gr_GR_cog.psa87.v2.number
psVE.tr.d070.prokeimenon3.v2.text = "Κύριε ὁ Θεὸς τῆς σωτηρίας μου, ἡμέρας ἐκέκραξα καὶ ἐν νυκτὶ ἐναντίον σου."
psVE.tr.d070.alleluia.title = "Ψαλμὸς 68."
psVE.tr.d070.alleluia.v1.info = ps_gr_GR_cog.psa68.v2.number
psVE.tr.d070.alleluia.v1.text = "Σῶσόν με, ὁ Θεός, ὅτι εἰσήλθοσαν ὕδατα ἕως ψυχῆς μου."
psVE.tr.d070.alleluia.v2.info = ps_gr_GR_cog.psa68.v22.number
psVE.tr.d070.alleluia.v2.text = "Ἔδωκαν εἰς τὸ βρῶμά μου χολήν, καὶ εἰς τὴν δίψαν μου ἐπότισάν με ὄξος."
psVE.tr.d070.alleluia.v3.info = ps_gr_GR_cog.psa68.v24.number
psVE.tr.d070.alleluia.v3.text = "Σκοτισθήτωσαν οἱ ὀφθαλμοὶ αὐτῶν τοῦ μὴ βλέπειν, καὶ τὸν νῶτον αὐτῶν διαπαντὸς σύγκαμψον."

psMA.tr.d070.prokeimenon.title = "Ψαλμὸς 9."
psMA.tr.d070.prokeimenon.v1.info = ps_gr_GR_cog.psa9.v33.number
psMA.tr.d070.prokeimenon.v1.text = "Ἀνάστηθι, Κύριε ὁ Θεός μου, ὑψωθήτω ἡ χείρ σου."
psMA.tr.d070.prokeimenon.v2.info = ps_gr_GR_cog.psa9.v2.number
psMA.tr.d070.prokeimenon.v2.text = "Ἐξομολογήσομαί σοι, Κύριε, ἐν ὅλῃ καρδίᾳ μου."
psMA.tr.d070.alleluia.title = "Ψαλμὸς 67."
psMA.tr.d070.alleluia.v1.info = ps_gr_GR_cog.psa67.v2.number
psMA.tr.d070.alleluia.v1.text = "Ἀναστήτω ὁ Θεός, καὶ διασκορπισθήτωσαν οἱ ἐχθροὶ αὐτοῦ."
psMA.tr.d070.alleluia.v2.info = ps_gr_GR_cog.psa67.v3.number
psMA.tr.d070.alleluia.v2.text = "Ὡς ἐκλείπει καπνός, ἐκλιπέτωσαν· ὡς τήκεται κηρὸς ἀπὸ προσώπου πυρός."
psMA.tr.d070.alleluia.v3.info = ps_gr_GR_cog.psa67.v3.number
psMA.tr.d070.alleluia.v3.text = "Οὕτως ἀπολοῦνται οἱ ἁμαρτωλοὶ ἀπὸ προσώπου τοῦ Θεοῦ."

psVL.tr.d071.prokeimenon.title = "Ψαλμὸς 65."
psVL.tr.d071.prokeimenon.v1.info = ps_gr_GR_cog.psa65.v4.number
psVL.tr.d071.prokeimenon.v1.text = "Πᾶσα ἡ γῆ προσκυνησάτωσάν σοι καὶ ψαλάτωσάν σοι."
psVL.tr.d071.prokeimenon.v2.info = ps_gr_GR_cog.psa65.v1.number
psVL.tr.d071.prokeimenon.v2.text = "Ἀλαλάξατε τῷ Κυρίῳ πᾶσα ἡ γῆ."
psVL.tr.d071.alleluia.title = "Ψαλμὸς 81."
psVL.tr.d071.alleluia.v0.info = ps_gr_GR_cog.psa81.v8.number
psVL.tr.d071.alleluia.v0.text = "Ἀνάστα, ὁ Θεός, κρίνων τὴν γῆν, ὅτι σὺ κατακληρονομήσεις ἐν πᾶσι τοῖς ἔθνεσι."
psVL.tr.d071.alleluia.v1.info = ps_gr_GR_cog.psa81.v1.number
psVL.tr.d071.alleluia.v1.text = "Ὁ Θεὸς ἔστη ἐν συναγωγῇ θεῶν, ἐν μέσῳ δὲ θεοὺς διακρινεῖ."
psVL.tr.d071.alleluia.v2.info = ps_gr_GR_cog.psa81.v2.number
psVL.tr.d071.alleluia.v2.text = "Ἕως πότε κρίνετε ἀδικίαν καὶ πρόσωπα ἁμαρτωλῶν λαμβάνετε;"
psVL.tr.d071.alleluia.v3.info = ps_gr_GR_cog.psa81.v3.number
psVL.tr.d071.alleluia.v3.text = "Κρίνατε ὀρφανῷ καὶ πτωχῷ, ταπεινὸν καὶ πένητα δικαιώσατε."
psVL.tr.d071.alleluia.v4.info = ps_gr_GR_cog.psa81.v4.number
psVL.tr.d071.alleluia.v4.text = "Ἐξέλεσθε πένητα καὶ πτωχόν, ἐκ χειρὸς ἁμαρτωλοῦ ρύσασθε αὐτόν."
psVL.tr.d071.alleluia.v5.info = ps_gr_GR_cog.psa81.v5.number
psVL.tr.d071.alleluia.v5.text = "Οὐκ ἔγνωσαν οὐδὲ συνῆκαν, ἐν σκότει διαπορεύονται· σαλευθήσονται πάντα τὰ θεμέλια τῆς γῆς."
psVL.tr.d071.alleluia.v6.info = ps_gr_GR_cog.psa81.v6.number
psVL.tr.d071.alleluia.v6.text = "Ἐγὼ εἶπα· θεοί ἐστε καὶ υἱοὶ Ὑψίστου πάντες· ὑμεῖς δὲ ὡς ἄνθρωποι ἀποθνήσκετε, καὶ ὡς εἷς τῶν ἀρχόντων πίπτετε."
psVL.tr.d071.communion.title = "Ψαλμὸς 77."
psVL.tr.d071.communion.v1.info = ps_gr_GR_cog.psa77.v65.number
psVL.tr.d071.communion.v1.text = "Ἐξηγέρθη ὡς ὁ ὑπνῶν Κύριος, καὶ ἀνέστη σώζων ἡμᾶς."

//Pentecostarion

psPA.stichera.v1.info = ps_gr_GR_cog.psa67.v2.number
psPA.stichera.v1.text = "Ἀναστήτω ὁ Θεός, καὶ διασκορπισθήτωσαν οἱ ἐχθροὶ αὐτοῦ, καὶ φυγέτωσαν ἀπὸ προσώπου αὐτοῦ οἱ μισοῦντες αὐτόν."
psPA.stichera.v2.info = ps_gr_GR_cog.psa67.v3.number
psPA.stichera.v2.text = "Ὡς ἐκλείπει καπνός, ἐκλιπέτωσαν· ὡς τήκεται κηρὸς ἀπὸ προσώπου πυρός."
psPA.stichera.v3.info = ps_gr_GR_cog.psa67.v3.number
psPA.stichera.v3.text = "Οὕτως ἀπολοῦνται οἱ ἁμαρτωλοὶ ἀπὸ προσώπου τοῦ Θεοῦ. Καὶ οἱ δίκαιοι εὐφρανθήτωσαν."
psPA.stichera.v4.info = ps_gr_GR_cog.psa117.v24.number
psPA.stichera.v4.text = "Αὕτη ἡ ἡμέρα, ἣν ἐποίησεν ὁ Κύριος· ἀγαλλιασώμεθα καὶ εὐφρανθῶμεν ἐν αὐτῇ."
psLI.pe.d071.antiphon1.title = "Ψαλμὸς 65."
psLI.pe.d071.antiphon1.v1.info = ps_gr_GR_cog.psa65.v1.number
psLI.pe.d071.antiphon1.v1.text = "Ἀλαλάξατε τῷ Κυρίῳ πᾶσα ἡ γῆ."
psLI.pe.d071.antiphon1.v2.info = ps_gr_GR_cog.psa65.v2.number
psLI.pe.d071.antiphon1.v2.text = "Ψάλατε δὴ τῷ ὀνόματι αὐτοῦ· δότε δόξαν ἐν αἰνέσει αὐτοῦ."
psLI.pe.d071.antiphon1.v3.info = ps_gr_GR_cog.psa65.v3.number
psLI.pe.d071.antiphon1.v3.text = "Εἴπατε τῷ Θεῷ· ὡς φοβερὰ τὰ ἔργα σου."
psLI.pe.d071.antiphon1.v4.info = ps_gr_GR_cog.psa65.v4.number
psLI.pe.d071.antiphon1.v4.text = "Πᾶσα ἡ γῆ προσκυνησάτωσάν σοι καὶ ψαλάτωσάν σοι."
psLI.pe.d071.antiphon2.title = "Ψαλμὸς 66."
psLI.pe.d071.antiphon2.v1.info = ps_gr_GR_cog.psa66.v2.number
psLI.pe.d071.antiphon2.v1.text = "Ὁ Θεὸς οἰκτειρήσαι ἡμᾶς καὶ εὐλογήσαι ἡμᾶς."
psLI.pe.d071.antiphon2.v2.info = ps_gr_GR_cog.psa66.v2.number
psLI.pe.d071.antiphon2.v2.text = "Ἐπιφάναι τὸ πρόσωπον αὑτοῦ ἐφʼ ἡμᾶς, καὶ ἐλεήσαι ἡμᾶς."
psLI.pe.d071.antiphon2.v3.info = ps_gr_GR_cog.psa66.v3.number
psLI.pe.d071.antiphon2.v3.text = "Τοῦ γνῶναι ἐν τῇ γῇ τὴν ὁδόν σου, ἐν πᾶσιν ἔθνεσι τὸ σωτήριόν σου."
psLI.pe.d071.antiphon2.v4.info = ps_gr_GR_cog.psa66.v4.number
psLI.pe.d071.antiphon2.v4.text = "Ἐξομολογησάσθωσάν σοι λαοί, ὁ Θεός, ἐξομολογησάσθωσάν σοι λαοὶ πάντες."
psLI.pe.d071.antiphon3.title = "Ψαλμὸς 67."
psLI.pe.d071.antiphon3.v1.info = ps_gr_GR_cog.psa67.v2.number
psLI.pe.d071.antiphon3.v1.text = "Ἀναστήτω ὁ Θεός, καὶ διασκορπισθήτωσαν οἱ ἐχθροὶ αὐτοῦ, καὶ φυγέτωσαν ἀπὸ προσώπου αὐτοῦ οἱ μισοῦντες αὐτόν."
psLI.pe.d071.antiphon3.v2.info = ps_gr_GR_cog.psa67.v3.number
psLI.pe.d071.antiphon3.v2.text = "Ὡς ἐκλείπει καπνός, ἐκλιπέτωσαν· ὡς τήκεται κηρὸς ἀπὸ προσώπου πυρός."
psLI.pe.d071.antiphon3.v3.info = ps_gr_GR_cog.psa67.v3.number
psLI.pe.d071.antiphon3.v3.text = "Οὕτως ἀπολοῦνται οἱ ἁμαρτωλοὶ ἀπὸ προσώπου τοῦ Θεοῦ. Καὶ οἱ δίκαιοι εὐφρανθήτωσαν."
psLI.pe.d071.antiphon3.v4.info = ps_gr_GR_cog.psa117.v24.number
psLI.pe.d071.antiphon3.v4.text = "Αὕτη ἡ ἡμέρα, ἣν ἐποίησεν ὁ Κύριος· ἀγαλλιασώμεθα καὶ εὐφρανθῶμεν ἐν αὐτῇ."
psLI.pe.d071.entrance.title = "Ψαλμὸς 67."
psLI.pe.d071.entrance.v1.info = ps_gr_GR_cog.psa67.v27.number
psLI.pe.d071.entrance.v1.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν, Κύριον ἐκ πηγῶν Ἰσραήλ."
psLI.pe.d071.prokeimenon.title = "Ψαλμὸς 117."
psLI.pe.d071.prokeimenon.v1.info = ps_gr_GR_cog.psa117.v24.number
psLI.pe.d071.prokeimenon.v1.text = "Αὕτη ἡ ἡμέρα, ἣν ἐποίησεν ὁ Κύριος· ἀγαλλιασώμεθα καὶ εὐφρανθῶμεν ἐν αὐτῇ."
psLI.pe.d071.prokeimenon.v2.info = ps_gr_GR_cog.psa117.v29.number
psLI.pe.d071.prokeimenon.v2.text = "Ἐξομολογεῖσθε τῷ Κυρίῳ, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.pe.d071.alleluia.title = "Ψαλμὸς 101."
psLI.pe.d071.alleluia.v1.info = ps_gr_GR_cog.psa101.v14.number
psLI.pe.d071.alleluia.v1.text = "Σύ, Κύριε, ἀναστὰς οἰκτειρήσεις τὴν Σιών."
psLI.pe.d071.alleluia.v2.info = ps_gr_GR_cog.psa101.v20.number
psLI.pe.d071.alleluia.v2.text = "Κύριος ἐξ οὐρανοῦ ἐπὶ τὴν γῆν ἐπέβλεψε."

psLI.pe.d072.prokeimenon.title = "Ψαλμὸς 18."
psLI.pe.d072.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.pe.d072.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτῶν."
psLI.pe.d072.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.pe.d072.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.pe.d072.alleluia.title = "Ψαλμὸς 88."
psLI.pe.d072.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.pe.d072.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.pe.d072.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.pe.d072.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.pe.d073.prokeimenon.title = "ᾨδὴ Ἐνάτη."
psLI.pe.d073.prokeimenon.v1.info = ps_gr_GR_cog.bode9_mag.v1.number
psLI.pe.d073.prokeimenon.v1.text = "Μεγαλύνει ἡ ψυχή μου τὸν Κύριον καὶ ἠγαλλίασε τὸ πνεῦμά μου."
psLI.pe.d073.prokeimenon.v2.info = ps_gr_GR_cog.bode9_mag.v2.number
psLI.pe.d073.prokeimenon.v2.text = "Ὅτι ἐπέβλεψεν ἐπὶ τὴν ταπείνωσιν τῆς δούλης αὐτοῦ."
psLI.pe.d073.alleluia.title = "Ψαλμὸς 131."
psLI.pe.d073.alleluia.v1.info = ps_gr_GR_cog.psa131.v8.number
psLI.pe.d073.alleluia.v1.text = "Ἀνάστηθι, Κύριε, εἰς τὴν ἀνάπαυσίν σου."
psLI.pe.d073.alleluia.v2.info = ps_gr_GR_cog.psa131.v11.number
psLI.pe.d073.alleluia.v2.text = "Ὤμοσε Κύριος τῷ Δαυῒδ ἀλήθειαν καὶ οὐ μὴ ἀθετήσει αὐτήν."

psVE.pe.d074.prokeimenon.title = "Ψαλμὸς 76."
psVE.pe.d074.prokeimenon.v1.info = ps_gr_GR_cog.psa76.v2.number
psVE.pe.d074.prokeimenon.v1.incipit = "Φωνῇ μου πρὸς Κύριον…"
psVE.pe.d074.prokeimenon.v1.text = "Φωνῇ μου πρὸς Κύριον ἐκέκραξα, φωνῇ μου πρὸς τὸν Θεόν, καὶ προσέσχε μοι."
psVE.pe.d074.prokeimenon.v2.info = ps_gr_GR_cog.psa76.v3.number
psVE.pe.d074.prokeimenon.v2.text = "Ἐν ἡμέρᾳ θλίψεώς μου τὸν Θεὸν ἐξεζήτησα."
psVE.pe.d074.prokeimenon.v3.info = ps_gr_GR_cog.psa76.v3.number
psVE.pe.d074.prokeimenon.v3.text = "Ἀπηνήνατο παρακληθῆναι ἡ ψυχή μου."
psVE.pe.d074.prokeimenon.v4.info = ps_gr_GR_cog.psa76.v14.number
psVE.pe.d074.prokeimenon.v4.text = "Ὁ Θεός, ἐν τῷ ἁγίῳ ἡ ὁδός σου."
psLI.pe.d074.prokeimenon.title = "Ψαλμὸς 44."
psLI.pe.d074.prokeimenon.v1.info = ps_gr_GR_cog.psa44.v18.number
psLI.pe.d074.prokeimenon.v1.text = "Μνησθήσομαι τοῦ ὀνόματός σου ἐν πάσῃ γενεᾷ καὶ γενεᾷ."
psLI.pe.d074.prokeimenon.v2.info = ps_gr_GR_cog.psa44.v11.number
psLI.pe.d074.prokeimenon.v2.text = "Ἄκουσον, θύγατερ, καὶ ἴδε καὶ κλῖνον τὸ οὖς σου."
psLI.pe.d074.alleluia.title = "ᾨδὴ Ἐνάτη."
psLI.pe.d074.alleluia.v1.info = ps_gr_GR_cog.bode9_mag.v1.number
psLI.pe.d074.alleluia.v1.text = "Μεγαλύνει ἡ ψυχή μου τὸν Κύριον καὶ ἠγαλλίασε τὸ πνεῦμά μου."
psLI.pe.d074.alleluia.v2.info = ps_gr_GR_cog.bode9_mag.v2.number
psLI.pe.d074.alleluia.v2.text = "Ὅτι ἐπέβλεψεν ἐπὶ τὴν ταπείνωσιν τῆς δούλης αὐτοῦ."

psVE.pe.d075.prokeimenon.title = "Ψαλμὸς 54."
psVE.pe.d075.prokeimenon.v1.info = ps_gr_GR_cog.psa54.v2.number
psVE.pe.d075.prokeimenon.v1.incipit = "Ἐνώτισαι, ὁ Θεός…"
psVE.pe.d075.prokeimenon.v1.text = "Ἐνώτισαι, ὁ Θεός, τὴν προσευχήν μου, καὶ μὴ ὑπερίδῃς τὴν δέησίν μου."
psVE.pe.d075.prokeimenon.v2.info = ps_gr_GR_cog.psa54.v3.number
psVE.pe.d075.prokeimenon.v2.text = "Πρόσχες μοι καὶ εἰσάκουσόν μου."
psVE.pe.d075.prokeimenon.v3.info = ps_gr_GR_cog.psa54.v4.number
psVE.pe.d075.prokeimenon.v3.text = "Ὅτι ἐξέκλιναν ἐπʼ ἐμὲ ἀνομίαν."
psVE.pe.d075.prokeimenon.v4.info = ps_gr_GR_cog.psa54.v17.number
psVE.pe.d075.prokeimenon.v4.text = "Ἐγὼ πρὸς τὸν Θεὸν ἐκέκραξα."
psLI.pe.d075.prokeimenon.title = "Ψαλμὸς 46."
psLI.pe.d075.prokeimenon.v1.info = ps_gr_GR_cog.psa46.v7.number
psLI.pe.d075.prokeimenon.v1.text = "Ψάλατε τῷ Θεῷ ἡμῶν, ψάλατε."
psLI.pe.d075.prokeimenon.v2.info = ps_gr_GR_cog.psa46.v2.number
psLI.pe.d075.prokeimenon.v2.text = "Πάντα τὰ ἔθνη κροτήσατε χεῖρας."
psLI.pe.d075.alleluia.title = "Ψαλμὸς 44."
psLI.pe.d075.alleluia.v1.info = ps_gr_GR_cog.psa44.v5.number
psLI.pe.d075.alleluia.v1.text = "Ἔντεινε καὶ κατευοδοῦ καὶ βασίλευε, ἕνεκεν ἀληθείας καὶ πρᾳότητος καὶ δικαιοσύνης."
psLI.pe.d075.alleluia.v2.info = ps_gr_GR_cog.psa44.v8.number
psLI.pe.d075.alleluia.v2.text = "Ἠγάπησας δικαιοσύνην καὶ ἐμίσησας ἀνομίαν."

psVE.pe.d076.prokeimenon.title = "Ψαλμὸς 17."
psVE.pe.d076.prokeimenon.v1.info = ps_gr_GR_cog.psa17.v2.number
psVE.pe.d076.prokeimenon.v1.incipit = "Ἀγαπήσω σε Κύριε…"
psVE.pe.d076.prokeimenon.v1.text = "Ἀγαπήσω σε Κύριε ἡ ἰσχύς μου, Κύριος στερέωμά μου, καὶ καταφυγή μου, καὶ ρύστης μου."
psVE.pe.d076.prokeimenon.v2.info = ps_gr_GR_cog.psa17.v3.number
psVE.pe.d076.prokeimenon.v2.text = "Ὁ Θεός μου βοηθός μου, ἐλπιῶ ἐπʼ αὐτόν."
psVE.pe.d076.prokeimenon.v3.info = ps_gr_GR_cog.psa17.v4.number
psVE.pe.d076.prokeimenon.v3.text = "Αἰνῶν ἐπικαλέσομαι τὸν Κύριον."
psVE.pe.d076.prokeimenon.v4.info = ps_gr_GR_cog.psa17.v7.number
psVE.pe.d076.prokeimenon.v4.text = "Ἤκουσεν ἐκ ναοῦ ἁγίου αὐτοῦ φωνῆς μου."
psVE.pe.d076.aposverse.v1.info = ps_gr_GR_cog.psa45.v5.number
psVE.pe.d076.aposverse.v1.text = "Ἡγίασε τὸ σκήνωμα αὐτοῦ ὁ Ὕψιστος."
psVE.pe.d076.aposverse.v2.info = ps_gr_GR_cog.psa45.v5.number
psVE.pe.d076.aposverse.v2.text = "Τοῦ ποταμοῦ τὰ ὁρμήματα εὐφραίνουσι τὴν πόλιν τοῦ Θεοῦ."
psLI.pe.d076.prokeimenon.title = "Ψαλμὸς 103."
psLI.pe.d076.prokeimenon.v1.info = ps_gr_GR_cog.psa103.v24.number
psLI.pe.d076.prokeimenon.v1.text = "Ὡς ἐμεγαλύνθη τὰ ἔργα σου, Κύριε· πάντα ἐν σοφίᾳ ἐποίησας."
psLI.pe.d076.prokeimenon.v2.info = ps_gr_GR_cog.psa103.v1.number
psLI.pe.d076.prokeimenon.v2.text = "Εὐλόγει, ἡ ψυχή μου, τὸν Κύριον."
psLI.pe.d076.alleluia.title = "Ψαλμὸς 88."
psLI.pe.d076.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.pe.d076.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.pe.d076.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.pe.d076.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.pe.d077.prokeimenon.title = "Ψαλμὸς 26."
psLI.pe.d077.prokeimenon.v1.info = ps_gr_GR_cog.psa26.v1.number
psLI.pe.d077.prokeimenon.v1.text = "Κύριος φωτισμός μου καὶ σωτήρ μου."
psLI.pe.d077.prokeimenon.v2.info = ps_gr_GR_cog.psa26.v1.number
psLI.pe.d077.prokeimenon.v2.text = "Κύριος ὑπερασπιστὴς τῆς ζωῆς μου."
psLI.pe.d077.alleluia.title = "Ψαλμὸς 92."
psLI.pe.d077.alleluia.v1.info = ps_gr_GR_cog.psa92.v1.number
psLI.pe.d077.alleluia.v1.text = "Ὁ Κύριος ἐβασίλευσεν, εὐπρέπειαν ἐνεδύσατο."
psLI.pe.d077.alleluia.v2.info = ps_gr_GR_cog.psa92.v1.number
psLI.pe.d077.alleluia.v2.text = "Καὶ γὰρ ἐστερέωσε τὴν οἰκουμένην, ἥτις οὐ σαλευθήσεται."

psVE.pe.d078.aposverse.v1.info = ps_gr_GR_cog.psa147.v1.number
psVE.pe.d078.aposverse.v1.text = "Ἐπαίνει, Ἰερουσαλήμ, τὸν Κύριον, αἴνει τὸν Θεόν σου, Σιών."
psVE.pe.d078.aposverse.v2.info = ps_gr_GR_cog.psa147.v2.number
psVE.pe.d078.aposverse.v2.text = "Ὅτι ἐνίσχυσε τοὺς μοχλοὺς τῶν πυλῶν σου, εὐλόγησε τοὺς υἱούς σου ἐν σοί."
psMA.pe.d078.prokeimenon.title = "Ψαλμὸς 147."
psMA.pe.d078.prokeimenon.v1.info = ps_gr_GR_cog.psa147.v1.number
psMA.pe.d078.prokeimenon.v1.text = "Ἐπαίνει, Ἰερουσαλήμ, τὸν Κύριον, αἴνει τὸν Θεόν σου, Σιών."
psMA.pe.d078.prokeimenon.v2.info = ps_gr_GR_cog.psa147.v2.number
psMA.pe.d078.prokeimenon.v2.text = "Ὅτι ἐνίσχυσε τοὺς μοχλοὺς τῶν πυλῶν σου, εὐλόγησε τοὺς υἱούς σου ἐν σοί."
psLI.pe.d078.prokeimenon.title = "Ψαλμοὶ 146, 134."
psLI.pe.d078.prokeimenon.v1.info = ps_gr_GR_cog.psa146.v5.number
psLI.pe.d078.prokeimenon.v1.text = "Μέγας ὁ Κύριος ἡμῶν, καὶ μεγάλη ἡ ἰσχὺς αὐτοῦ."
psLI.pe.d078.prokeimenon.v2.info = ps_gr_GR_cog.psa134.v3.number
psLI.pe.d078.prokeimenon.v2.text = "Αἰνεῖτε τὸν Κύριον, ὅτι ἀγαθὸς Κύριος."
psLI.pe.d078.alleluia.title = "Ψαλμὸς 94."
psLI.pe.d078.alleluia.v1.info = ps_gr_GR_cog.psa94.v1.number
psLI.pe.d078.alleluia.v1.text = "Δεῦτε ἀγαλλιασώμεθα τῷ Κυρίῳ."
psLI.pe.d078.alleluia.v2.info = ps_gr_GR_cog.psa94.v3.number
psLI.pe.d078.alleluia.v2.text = "Ὅτι Θεὸς μέγας Κύριος."
psLI.pe.d078.communion.title = "Ψαλμὸς 147."
psLI.pe.d078.communion.v1.info = ps_gr_GR_cog.psa147.v1.number
psLI.pe.d078.communion.v1.text = "Ἐπαίνει, Ἰερουσαλήμ, τὸν Κύριον· αἴνει τὸν Θεόν σου, Σιών."

psMA.pe.myrrhbearers.aposverse.v1.info = ps_gr_GR_cog.psa84.v2.number
psMA.pe.myrrhbearers.aposverse.v1.text = "Εὐδόκησας, Κύριε, τὴν γῆν σου, ἀπέστρεψας τὴν αἰχμαλωσίαν Ἰακώβ."
psMA.pe.myrrhbearers.aposverse.v2.info = ps_gr_GR_cog.psa84.v11.number
psMA.pe.myrrhbearers.aposverse.v2.text = "Ἔλεος καὶ ἀλήθεια συνήντησαν, δικαιοσύνη καὶ εἰρήνη κατεφίλησαν."

psMA.pe.paralytic.aposverse.v1.info = ps_gr_GR_cog.psa88.v2.number
psMA.pe.paralytic.aposverse.v1.text = "Τὰ ἐλέη σου, Κύριε, εἰς τὸν αἰῶνα ᾄσομαι."
psMA.pe.paralytic.aposverse.v2.info = ps_gr_GR_cog.psa88.v3.number
psMA.pe.paralytic.aposverse.v2.text = "Ὅτι εἶπας· Εἰς τὸν αἰῶνα ἔλεος οἰκοδομηθήσεται."

psMA.pe.samaritanwoman.aposverse.v1.info = ps_gr_GR_cog.psa44.v5.number
psMA.pe.samaritanwoman.aposverse.v1.text = "Ἔντεινε καὶ κατευοδοῦ καὶ βασίλευε ἕνεκεν ἀληθείας καὶ χρηστότητος."
psMA.pe.samaritanwoman.aposverse.v2.info = ps_gr_GR_cog.psa44.v8.number
psMA.pe.samaritanwoman.aposverse.v2.text = "Ἠγάπησας δικαιοσύνην καὶ ἐμίσησας ἀνομίαν."

psVE.pe.d095.aposverse.v1.info = ps_gr_GR_cog.psa73.v2.number
psVE.pe.d095.aposverse.v1.text = "Μνήσθητι τῆς συναγωγῆς σου, ἧς ἐκτήσω ἀπʼ ἀρχῆς."
psVE.pe.d095.aposverse.v2.info = ps_gr_GR_cog.psa73.v12.number
psVE.pe.d095.aposverse.v2.text = "Ὁ δὲ Θεὸς βασιλεὺς ἡμῶν πρὸ αἰώνων, εἰργάσατο σωτηρίαν ἐν μέσῳ τῆς γῆς."
psLI.pe.d095.prokeimenon.title = "Ψαλμὸς 73."
psLI.pe.d095.prokeimenon.v1.info = ps_gr_GR_cog.psa73.v2.number
psLI.pe.d095.prokeimenon.v1.text = "Μνήσθητι τῆς συναγωγῆς σου, ἧς ἐκτήσω ἀπʼ ἀρχῆς."
psLI.pe.d095.prokeimenon.v2.info = ps_gr_GR_cog.psa73.v12.number
psLI.pe.d095.prokeimenon.v2.text = "Ὁ δὲ Θεὸς βασιλεὺς ἡμῶν πρὸ αἰώνων, εἰργάσατο σωτηρίαν ἐν μέσῳ τῆς γῆς."
//Alleluia verses according to Church of Greece
psLI.pe.d095.alleluia.title = "Ψαλμὸς 146."
psLI.pe.d095.alleluia.v1.info = ps_gr_GR_cog.psa146.v5.number
psLI.pe.d095.alleluia.v1.text = "Μέγας ὁ Κύριος ἡμῶν, καὶ μεγάλη ἡ ἰσχὺς αὐτοῦ."
psLI.pe.d095.alleluia.v2.info = ps_gr_GR_cog.psa146.v1.number
psLI.pe.d095.alleluia.v2.text = "Αἰνεῖτε τὸν Κύριον, ὅτι ἀγαθὸς ψαλμός."
psMA.pe.midpentecost.aposverse.v1.info = ps_gr_GR_cog.psa73.v2.number
psMA.pe.midpentecost.aposverse.v1.text = "Μνήσθητι τῆς συναγωγῆς σου, ἧς ἐκτήσω ἀπʼ ἀρχῆς."
psMA.pe.midpentecost.aposverse.v2.info = ps_gr_GR_cog.psa73.v12.number
psMA.pe.midpentecost.aposverse.v2.text = "Ὁ δὲ Θεὸς βασιλεὺς ἡμῶν πρὸ αἰώνων, εἰργάσατο σωτηρίαν ἐν μέσῳ τῆς γῆς."

psMA.pe.blindman.aposverse.v1.info = ps_gr_GR_cog.psa118.v132.number
psMA.pe.blindman.aposverse.v1.text = "Ἐπίβλεψον ἐπʼ ἐμὲ καὶ ἐλέησόν με."
psMA.pe.blindman.aposverse.v2.info = ps_gr_GR_cog.psa118.v133.number
psMA.pe.blindman.aposverse.v2.text = "Τὰ διαβήματά μου κατεύθυνον κατὰ τὸ λόγιόν σου."

psVE.pe.d110.aposverse.v1.info = ps_gr_GR_cog.psa46.v6.number
psVE.pe.d110.aposverse.v1.text = "Ἀνέβη ὁ Θεὸς ἐν ἀλαλαγμῷ, Κύριος ἐν φωνῇ σάλπιγγος."
psVE.pe.d110.aposverse.v2.info = ps_gr_GR_cog.psa46.v2.number
psVE.pe.d110.aposverse.v2.text = "Πάντα τὰ ἔθνη κροτήσατε χεῖρας, ἀλαλάξατε τῷ Θεῷ ἐν φωνῇ ἀγαλλιάσεως."
psMA.pe.d110.prokeimenon.title = "Ψαλμὸς 46."
psMA.pe.d110.prokeimenon.v1.info = ps_gr_GR_cog.psa46.v6.number
psMA.pe.d110.prokeimenon.v1.text = "Ἀνέβη ὁ Θεὸς ἐν ἀλαλαγμῷ, Κύριος ἐν φωνῇ σάλπιγγος."
psMA.pe.d110.prokeimenon.v2.info = ps_gr_GR_cog.psa46.v2.number
psMA.pe.d110.prokeimenon.v2.text = "Πάντα τὰ ἔθνη κροτήσατε χεῖρας, ἀλαλάξατε τῷ Θεῷ ἐν φωνῇ ἀγαλλιάσεως."
psLI.pe.d110.antiphon1.title = "Ψαλμὸς 46."
psLI.pe.d110.antiphon1.v1.info = ps_gr_GR_cog.psa46.v2.number
psLI.pe.d110.antiphon1.v1.text = "Πάντα τὰ ἔθνη κροτήσατε χεῖρας· ἀλαλάξατε τῷ Θεῷ ἐν φωνῇ ἀγαλλιάσεως."
psLI.pe.d110.antiphon1.v2.info = ps_gr_GR_cog.psa46.v3.number
psLI.pe.d110.antiphon1.v2.text = "Ὅτι Κύριος ὕψιστος, φοβερός, Βασιλεὺς μέγας ἐπὶ πᾶσαν τὴν γῆν."
psLI.pe.d110.antiphon1.v3.info = ps_gr_GR_cog.psa46.v4.number
psLI.pe.d110.antiphon1.v3.text = "Ὑπέταξε λαοὺς ἡμῖν, καὶ ἔθνη ὑπὸ τοὺς πόδας ἡμῶν."
psLI.pe.d110.antiphon2.title = "Ψαλμὸς 47."
psLI.pe.d110.antiphon2.v1.info = ps_gr_GR_cog.psa47.v2.number
psLI.pe.d110.antiphon2.v1.text = "Μέγας Κύριος καὶ αἰνετὸς σφόδρα."
psLI.pe.d110.antiphon2.v2.info = ps_gr_GR_cog.psa47.v4.number
psLI.pe.d110.antiphon2.v2.text = "Ὁ Θεὸς ἐν τοῖς βάρεσιν αὐτῆς γινώσκεται."
psLI.pe.d110.antiphon2.v3.info = ps_gr_GR_cog.psa47.v5.number
psLI.pe.d110.antiphon2.v3.text = "Ὅτι ἰδοὺ οἱ βασιλεῖς τῆς γῆς συνήχθησαν."
psLI.pe.d110.antiphon3.title = "Ψαλμὸς 48."
psLI.pe.d110.antiphon3.v1.info = ps_gr_GR_cog.psa48.v2.number
psLI.pe.d110.antiphon3.v1.text = "Ἀκούσατε ταῦτα, πάντα τὰ ἔθνη."
psLI.pe.d110.antiphon3.v2.info = ps_gr_GR_cog.psa48.v4.number
psLI.pe.d110.antiphon3.v2.text = "Τὸ στόμα μου λαλήσει σοφίαν, καὶ ἡ μελέτη τῆς καρδίας μου σύνεσιν."
psLI.pe.d110.antiphon3.v3.info = ps_gr_GR_cog.psa48.v5.number
psLI.pe.d110.antiphon3.v3.text = "Κλινῶ εἰς παραβολὴν τὸ οὖς μου, ἀνοίξω ἐν ψαλτηρίῳ τὸ πρόβλημά μου."
psLI.pe.d110.entrance.title = "Ψαλμὸς 46."
psLI.pe.d110.entrance.v1.info = ps_gr_GR_cog.psa46.v6.number
psLI.pe.d110.entrance.v1.text = "Ἀνέβη ὁ Θεὸς ἐν ἀλαλαγμῷ, Κύριος ἐν φωνῇ σάλπιγγος."
psLI.pe.d110.prokeimenon.title = "Ψαλμὸς 107."
psLI.pe.d110.prokeimenon.v1.info = ps_gr_GR_cog.psa107.v6.number
psLI.pe.d110.prokeimenon.v1.text = "Ὑψώθητι ἐπὶ τοὺς οὐρανούς, ὁ Θεός."
psLI.pe.d110.prokeimenon.v2.info = ps_gr_GR_cog.psa107.v2.number
psLI.pe.d110.prokeimenon.v2.text = "Ἑτοίμη ἡ καρδία μου, ὁ Θεός, ἑτοίμη ἡ καρδία μου."
psLI.pe.d110.alleluia.title = "Ψαλμὸς 46."
psLI.pe.d110.alleluia.v1.info = ps_gr_GR_cog.psa46.v2.number
psLI.pe.d110.alleluia.v1.text = "Πάντα τὰ ἔθνη κροτήσατε χεῖρας."
psLI.pe.d110.alleluia.v2.info = ps_gr_GR_cog.psa46.v2.number
psLI.pe.d110.alleluia.v2.text = "Ἀλαλάξατε τῷ Θεῷ ἐν φωνῇ ἀγαλλιάσεως."
psLI.pe.d110.communion.title = "Ψαλμὸς 46."
psLI.pe.d110.communion.v1.info = ps_gr_GR_cog.psa46.v6.number
psLI.pe.d110.communion.v1.text = "Ἀνέβη ὁ Θεὸς ἐν ἀλαλαγμῷ, Κύριος ἐν φωνῇ σάλπιγγος."

psLI.pe.d113.prokeimenon.title = "Δανιὴλ 3."
psLI.pe.d113.prokeimenon.v1.info = le.pr.os_gr_GR_cog.leosLI.pe.d113.EpistleProkeimenon.book_abbr
psLI.pe.d113.prokeimenon.v1.text = "Εὐλογητὸς εἶ Κύριε ὁ Θεὸς τῶν πατέρων ἡμῶν."
psLI.pe.d113.prokeimenon.v2.info = le.pr.os_gr_GR_cog.leosLI.pe.d113.EpistleProkeimenon.chapverse
psLI.pe.d113.prokeimenon.v2.text = "Ὅτι δίκαιος εἶ ἐπὶ πᾶσιν, οἷς ἐποίησας ἡμῖν."
psLI.pe.d113.alleluia.title = "Ψαλμὸς 49."
psLI.pe.d113.alleluia.v1.info = ps_gr_GR_cog.psa49.v1.number
psLI.pe.d113.alleluia.v1.text = "Θεὸς θεῶν Κύριος ἐλάλησε καὶ ἐκάλεσε τὴν γῆν."
psLI.pe.d113.alleluia.v2.info = ps_gr_GR_cog.psa49.v5.number
psLI.pe.d113.alleluia.v2.text = "Συναγάγετε αὐτῷ τοὺς ὁσίους αὐτοῦ."

psLI.pe.d119.prokeimenon.title = "Ψαλμοὶ 24, 27."
psLI.pe.d119.prokeimenon.v1.info = ps_gr_GR_cog.psa24.v13.number
psLI.pe.d119.prokeimenon.v1.text = "Αἱ ψυχαὶ αὐτῶν ἐν ἀγαθοῖς αὐλισθήσονται."
psLI.pe.d119.prokeimenon.v2.info = ps_gr_GR_cog.psa27.v1.number
psLI.pe.d119.prokeimenon.v2.text = "Πρὸς σέ, Κύριε, κεκράξομαι, ὁ Θεός μου."
psLI.pe.d119.alleluia.title = "Ψαλμοὶ 64, 101."
psLI.pe.d119.alleluia.v1.info = ps_gr_GR_cog.psa64.v5.number
psLI.pe.d119.alleluia.v1.text = "Μακάριοι οὓς ἐξελέξω καὶ προσελάβου, Κύριε."
psLI.pe.d119.alleluia.v2.info = ps_gr_GR_cog.psa101.v13.number
psLI.pe.d119.alleluia.v2.text = "Τὸ μνημόσυνον αὐτῶν εἰς γενεὰν καὶ γενεάν."

psVE.pe.d120.aposverse.v1.info = ps_gr_GR_cog.psa50.v12.number
psVE.pe.d120.aposverse.v1.text = "Καρδίαν καθαρὰν κτίσον ἐν ἐμοί, ὁ Θεός, καὶ πνεῦμα εὐθὲς ἐγκαίνισον ἐν τοῖς ἐγκάτοις μου."
psVE.pe.d120.aposverse.v2.info = ps_gr_GR_cog.psa50.v13.number
psVE.pe.d120.aposverse.v2.text = "Μὴ ἀποῤῥίψῃς με ἀπὸ τοῦ προσώπου σου καὶ τὸ πνεῦμά σου τὸ ἅγιον μὴ ἀντανέλῃς ἀπʼ ἐμοῦ."
psMA.pe.d120.prokeimenon.title = "Ψαλμὸς 142."
psMA.pe.d120.prokeimenon.v1.info = ps_gr_GR_cog.psa142.v10.number
psMA.pe.d120.prokeimenon.v1.text = "Τὸ πνεῦμά σου τὸ ἀγαθὸν ὁδηγήσει με ἐν γῇ εὐθείᾳ."
psMA.pe.d120.prokeimenon.v2.info = ps_gr_GR_cog.psa142.v1.number
psMA.pe.d120.prokeimenon.v2.text = "Κύριε, εἰσάκουσον τῆς προσευχῆς μου, ἐνώτισαι τὴν δέησίν μου ἐν τῇ ἀληθείᾳ σου."
psLI.pe.d120.antiphon1.title = "Ψαλμὸς 18."
psLI.pe.d120.antiphon1.v1.info = ps_gr_GR_cog.psa18.v2.number
psLI.pe.d120.antiphon1.v1.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ, ποίησιν δὲ χειρῶν αὐτοῦ ἀναγγέλλει τὸ στερέωμα."
psLI.pe.d120.antiphon1.v2.info = ps_gr_GR_cog.psa18.v3.number
psLI.pe.d120.antiphon1.v2.text = "Ἡμέρα τῇ ἡμέρᾳ ἐρεύγεται ῥῆμα, καὶ νὺξ νυκτὶ ἀναγγέλλει γνῶσιν."
psLI.pe.d120.antiphon1.v3.info = ps_gr_GR_cog.psa18.v4.number
psLI.pe.d120.antiphon1.v3.text = "Οὔκ εἰσι λαλιαὶ οὐδὲ λόγοι, ὧν οὐχὶ ἀκούονται αἱ φωναὶ αὐτῶν."
psLI.pe.d120.antiphon1.v4.info = ps_gr_GR_cog.psa18.v5.number
psLI.pe.d120.antiphon1.v4.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτῶν, καὶ εἰς τὰ πέρατα τῆς οἰκουμένης τὰ ῥήματα αὐτῶν."
psLI.pe.d120.antiphon2.title = "Ψαλμὸς 19."
psLI.pe.d120.antiphon2.v1.info = ps_gr_GR_cog.psa19.v2.number
psLI.pe.d120.antiphon2.v1.text = "Ἐπακούσαι σου Κύριος ἐν ἡμέρᾳ θλίψεως, ὑπερασπίσαι σου τὸ ὄνομα τοῦ Θεοῦ Ἰακώβ."
psLI.pe.d120.antiphon2.v2.info = ps_gr_GR_cog.psa19.v3.number
psLI.pe.d120.antiphon2.v2.text = "Ἐξαποστείλαι σοι βοήθειαν ἐξ ἁγίου, καὶ ἐκ Σιὼν ἀντιλάβοιτό σου."
psLI.pe.d120.antiphon2.v3.info = ps_gr_GR_cog.psa19.v4.number
psLI.pe.d120.antiphon2.v3.text = "Μνησθείη πάσης θυσίας σου, καὶ τὸ ὁλοκαύτωμά σου πιανάτω."
psLI.pe.d120.antiphon2.v4.info = ps_gr_GR_cog.psa19.v5.number
psLI.pe.d120.antiphon2.v4.text = "Δῴη σοι Κύριος κατὰ τὴν καρδίαν σου, καὶ πᾶσαν τὴν βουλήν σου πληρώσαι."
psLI.pe.d120.antiphon3.title = "Ψαλμὸς 20."
psLI.pe.d120.antiphon3.v1.info = ps_gr_GR_cog.psa20.v2.number
psLI.pe.d120.antiphon3.v1.text = "Κύριε, ἐν τῇ δυνάμει σου εὐφρανθήσεται ὁ Βασιλεύς, καὶ ἐπὶ τῷ σωτηρίῳ σου ἀγαλλιάσεται σφόδρα."
psLI.pe.d120.antiphon3.v2.info = ps_gr_GR_cog.psa20.v3.number
psLI.pe.d120.antiphon3.v2.text = "Τὴν ἐπιθυμίαν τῆς καρδίας αὐτοῦ ἔδωκας αὐτῷ, καὶ τὴν θέλησιν τῶν χειλέων αὐτοῦ οὐκ ἐστέρησας αὐτόν."
psLI.pe.d120.antiphon3.v3.info = ps_gr_GR_cog.psa20.v4.number
psLI.pe.d120.antiphon3.v3.text = "Ὅτι προέφθασας αὐτὸν ἐν εὐλογίαις χρηστότητος, ἔθηκας ἐπὶ τὴν κεφαλὴν αὐτοῦ στέφανον ἐκ λίθου τιμίου."
psLI.pe.d120.antiphon3.v4.info = ps_gr_GR_cog.psa20.v5.number
psLI.pe.d120.antiphon3.v4.text = "Ζωὴν ᾐτήσατό σε, καὶ ἔδωκας αὐτῷ μακρότητα ἡμερῶν εἰς αἰῶνα αἰῶνος."
psLI.pe.d120.entrance.title = "Ψαλμὸς 20."
psLI.pe.d120.entrance.v1.info = ps_gr_GR_cog.psa20.v14.number
psLI.pe.d120.entrance.v1.text = "Ὑψώθητι, Κύριε, ἐν τῇ δυνάμει σου, ᾄσομεν καὶ ψαλοῦμεν τὰς δυναστείας σου."
psLI.pe.d120.prokeimenon.title = "Ψαλμὸς 18."
psLI.pe.d120.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.pe.d120.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτῶν, καὶ εἰς τὰ πέρατα τῆς οἰκουμένης τὰ ῥήματα αὐτῶν."
psLI.pe.d120.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.pe.d120.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ, ποίησιν δὲ χειρῶν αὐτοῦ ἀναγγέλλει τὸ στερέωμα."
psLI.pe.d120.alleluia.title = "Ψαλμὸς 32."
psLI.pe.d120.alleluia.v1.info = ps_gr_GR_cog.psa32.v6.number
psLI.pe.d120.alleluia.v1.text = "Τῷ λόγῳ τοῦ Κυρίου οἱ οὐρανοὶ ἐστερεώθησαν, καὶ τῷ πνεύματι τοῦ στόματος αὐτοῦ πᾶσα ἡ δύναμις αὐτῶν."
psLI.pe.d120.alleluia.v2.info = ps_gr_GR_cog.psa32.v13.number
psLI.pe.d120.alleluia.v2.text = "Ἐξ οὐρανοῦ ἐπέβλεψεν ὁ Κύριος· εἶδε πάντας τοὺς υἱοὺς τῶν ἀνθρώπων."
psLI.pe.d120.communion.title = "Ψαλμὸς 142."
psLI.pe.d120.communion.v1.info = ps_gr_GR_cog.psa142.v10.number
psLI.pe.d120.communion.v1.text = "Τὸ πνεῦμά σου τὸ ἀγαθὸν ὁδηγήσει με ἐν γῇ εὐθείᾳ."

psLI.pe.d121.prokeimenon.title = "Ψαλμὸς 27."
psLI.pe.d121.prokeimenon.v1.info = ps_gr_GR_cog.psa27.v9.number
psLI.pe.d121.prokeimenon.v1.text = "Σῶσον, Κύριε, τὸν λαόν σου καὶ εὐλόγησον τὴν κληρονομίαν σου."
psLI.pe.d121.prokeimenon.v2.info = ps_gr_GR_cog.psa27.v1.number
psLI.pe.d121.prokeimenon.v2.text = "Πρὸς σέ, Κύριε, κεκράξομαι, ὁ Θεός μου."
psLI.pe.d121.alleluia.title = "Ψαλμὸς 50."
psLI.pe.d121.alleluia.v1.info = ps_gr_GR_cog.psa50.v3.number
psLI.pe.d121.alleluia.v1.text = "Ἐλέησόν με, ὁ Θεός, κατὰ τὸ μέγα ἔλεός σου."
psLI.pe.d121.alleluia.v2.info = ps_gr_GR_cog.psa50.v13.number
psLI.pe.d121.alleluia.v2.text = "Μὴ ἀποῤῥίψῃς με ἀπὸ τοῦ προσώπου σου καὶ τὸ Πνεῦμά σου τὸ ἅγιον μὴ ἀντανέλῃς ἀπʼ ἐμοῦ."

psLI.pe.d122.prokeimenon.title = "Ψαλμοὶ 146, 32."
psLI.pe.d122.prokeimenon.v1.info = ps_gr_GR_cog.psa146.v5.number
psLI.pe.d122.prokeimenon.v1.text = "Μέγας ὁ Κύριος ἡμῶν, καὶ μεγάλη ἡ ἰσχὺς αὐτοῦ."
psLI.pe.d122.prokeimenon.v2.info = ps_gr_GR_cog.psa32.v6.number
psLI.pe.d122.prokeimenon.v2.text = "Τῷ λόγῳ τοῦ Κυρίου οἱ οὐρανοὶ ἐστερεώθησαν καὶ τῷ πνεύματι τοῦ στόματος αὐτοῦ πᾶσα ἡ δύναμις αὐτῶν."
psLI.pe.d122.alleluia.title = "Ψαλμὸς 88."
psLI.pe.d122.alleluia.v1.info = ps_gr_GR_cog.psa88.v2.number
psLI.pe.d122.alleluia.v1.text = "Τὰ ἐλέη σου, Κύριε, εἰς τὸν αἰῶνα ᾄσομαι."
psLI.pe.d122.alleluia.v2.info = ps_gr_GR_cog.psa88.v3.number
psLI.pe.d122.alleluia.v2.text = "Ὅτι εἶπας· Εἰς τὸν αἰῶνα ἔλεος οἰκοδομηθήσεται."

psMA.pe.d127.laudsverse.v1.info = ps_gr_GR_cog.psa33.v18.number
psMA.pe.d127.laudsverse.v1.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."
psMA.pe.d127.laudsverse.v2.info = ps_gr_GR_cog.psa67.v36.number
psMA.pe.d127.laudsverse.v2.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.pe.d127.prokeimenon.title = "Ψαλμὸς 67."
psLI.pe.d127.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.pe.d127.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.pe.d127.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.pe.d127.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.pe.d127.alleluia.title = "Ψαλμὸς 33."
psLI.pe.d127.alleluia.v1.info = ps_gr_GR_cog.psa33.v18.number
psLI.pe.d127.alleluia.v1.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."
psLI.pe.d127.alleluia.v2.info = ps_gr_GR_cog.psa33.v20.number
psLI.pe.d127.alleluia.v2.text = "Πολλαὶ αἱ θλίψεις τῶν δικαίων, καὶ ἐκ πασῶν αὐτῶν ῥύσεται αὐτοὺς ὁ Κύριος."
