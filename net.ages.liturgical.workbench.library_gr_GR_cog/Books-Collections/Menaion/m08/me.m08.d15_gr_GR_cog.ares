meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C11.greekmelody = he.h.m1_gr_GR_cog.heHE.NenikintaiTisFyseos.greekname
meMA.Ode9C11.melody = he.h.m1_gr_GR_cog.heHE.NenikintaiTisFyseos.name
meMA.Ode9C11.incipit = "Ἐξίσταντο Ἀγγέλων αἱ δυνάμεις…"
meMA.Ode9C11.text = "Ἐξίσταντο Ἀγγέλων αἱ δυνάμεις, ἐν τῇ Σιὼν σκοπούμεναι, τὸν οἰκεῖον Δεσπότην, γυναικείαν ψυχὴν χειριζόμενον· τῇ γὰρ ἀχράντως τεκούσῃ, υἱοπρεπῶς προσεφώνει· Δεῦρο Σεμνή, τῷ Υἱῷ καὶ Θεῷ συνδοξάσθητι."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C12.melody = he.h.m1_gr_GR_cog.heHE.NenikintaiTisFyseos.name
meMA.Ode9C12.incipit = "Συνέστειλε χορὸς τῶν Ἀποστόλων, τὸ…"
meMA.Ode9C12.text = "Συνέστειλε χορὸς τῶν Ἀποστόλων, τὸ θεοδόχον Σῶμά σου, μετὰ δέους ὁρῶντες, καὶ φωνῇ λιγυρᾷ προσφθεγγόμενοι· Εἰς οὐρανίους θαλάμους, πρὸς τὸν Υἱὸν ἐκφοιτῶσα, σῴζοις ἀεί, Θεοτόκε τὴν κληρονομίαν σου."

meMA.Ode9C2M01.incipit = "Ἄγγελοι τὴν Κοίμησιν…"
meMA.Ode9C2M01.text = "Ἄγγελοι τὴν Κοίμησιν τῆς Παρθένου, ὁρῶντες ἐξεπλήττοντο, πῶς ἡ Παρθένος ἀπαίρει, ἀπὸ τῆς γῆς εἰς τὰ ἄνω."

meMA.Ode9C2H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C2H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C2H.greekmelody = he.h.m4_gr_GR_cog.heHE.ApasGigenisKoimisis.greekname
meMA.Ode9C2H.melody = he.h.m4_gr_GR_cog.heHE.ApasGigenisKoimisis.melody
meMA.Ode9C2H.incipit = he.h.m4_gr_GR_cog.heHE.ApasGigenisKoimisis.incipit
meMA.Ode9C2H.text = he.h.m4_gr_GR_cog.heHE.ApasGigenisKoimisis.text

meMA.Ode9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C21.greekmelody = he.h.m4_gr_GR_cog.heHE.ApasGigenisKoimisis.greekname
meMA.Ode9C21.melody = he.h.m4_gr_GR_cog.heHE.ApasGigenisKoimisis.name
meMA.Ode9C21.incipit = "Δεῦτε ἐν Σιών…"
meMA.Ode9C21.text = "Δεῦτε ἐν Σιών, τῷ θείῳ καὶ πίονι, ὄρει τοῦ ζῶντος Θεοῦ, ἀγαλλιασώμεθα, τὴν Θεοτόκον ἐνοπτριζόμενοι· πρὸς γὰρ τὴν λίαν κρείττονα, καὶ θειοτέραν σκηνήν, ὡς Μητέρα, ταύτην εἰς τὰ Ἅγια, τῶν Ἁγίων Χριστὸς μετατίθησι."

meMA.Ode9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C22.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C22.melody = he.h.m4_gr_GR_cog.heHE.ApasGigenisKoimisis.name
meMA.Ode9C22.incipit = "Δεῦτε οἱ πιστοί, τῷ τάφῳ…"
meMA.Ode9C22.text = "Δεῦτε οἱ πιστοί, τῷ τάφῳ προσέλθωμεν, τῆς Θεομήτορος, καὶ περιπτυξώμεθα, καρδίας χείλη ὄμματα μέτωπα, εἰλικρινῶς προσάπτοντες, καὶ ἀρυσώμεθα, ἰαμάτων, ἄφθονα χαρίσματα, ἐκ πηγῆς ἀενάου βλυστάνοντα."

meMA.Ode9C23.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C23.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C23.melody = he.h.m4_gr_GR_cog.heHE.ApasGigenisKoimisis.name
meMA.Ode9C23.incipit = "Δέχου παρʼ ἡμῶν, ᾠδὴν τὴν…"
meMA.Ode9C23.text = "Δέχου παρʼ ἡμῶν, ᾠδὴν τὴν ἐξόδιον, Μῆτερ τοῦ ζῶντος Θεοῦ, καὶ τῇ φωτοφόρῳ σου, καὶ θείᾳ ἐπισκίασον χάριτι, τῷ Βασιλεῖ τὰ τρόπαια, τῷ φιλοχρίστῳ λαῷ, τὴν εἰρήνην, ἄφεσιν τοῖς μέλπουσι, καὶ ψυχῶν σωτηρίαν βραβεύουσα."

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Exaposteilarion1.greekmelody = he.a.m3_gr_GR_cog.heAU.ApostoloiEkPeraton.greekname
meMA.Exaposteilarion1.melody = he.a.m3_gr_GR_cog.heAU.ApostoloiEkPeraton.melody
meMA.Exaposteilarion1.incipit = he.a.m3_gr_GR_cog.heAU.ApostoloiEkPeraton.incipit
meMA.Exaposteilarion1.text = he.a.m3_gr_GR_cog.heAU.ApostoloiEkPeraton.text

meMA.Lauds1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Lauds1.greekmelody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.greekname
meMA.Lauds1.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meMA.Lauds1.incipit = "Τῇ ἐνδόξῳ Κοιμήσει σου…"
meMA.Lauds1.text = "Τῇ ἐνδόξῳ Κοιμήσει σου, οὐρανοὶ ἐπαγάλλονται, καὶ Ἀγγέλων γέγηθε τὰ στρατεύματα· πᾶσα ἡ γῆ δὲ εὐφραίνεται, ᾠδήν σοι ἐξόδιον, προσφωνοῦσα τῇ Μητρί, τοῦ τῶν ὅλων δεσπόζοντος, ἀπειρόγαμε, Παναγία Παρθένε, ἡ τὸ γένος, τῶν ἀνθρώπων ῥυσαμένη, προγονικῆς ἀποφάσεως."

meMA.Lauds2.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Lauds2.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meMA.Lauds2.incipit = "Ἐκ περάτων συνέδραμον…"
meMA.Lauds2.text = "Ἐκ περάτων συνέδραμον, Ἀποστόλων οἱ πρόκριτοι, θεαρχίῳ νεύματι τοῦ κηδεῦσαί σε· καὶ ἀπὸ γῆς αἰρομένην σε, πρὸς ὕψος θεώμενοι, τὴν φωνὴν τοῦ Γαβριήλ, ἐν χαρᾷ ἀνεβόων σοι· Χαῖρε ὄχημα, τῆς Θεότητος ὅλης· χαῖρε μόνη, τὰ ἐπίγεια τοῖς ἄνω, τῷ τοκετῷ σου συνάψασα."

meMA.Lauds3.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Lauds3.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meMA.Lauds3.incipit = "Τὴν ζωὴν ἡ κυήσασα…"
meMA.Lauds3.text = "Τὴν ζωὴν ἡ κυήσασα, πρὸς ζωὴν μεταβέβηκας, τῇ σεπτῇ Κοιμήσει σου τὴν ἀθάνατον, δορυφορούντων Ἀγγέλων σοι, Ἀρχῶν καὶ Δυνάμεων, Ἀποστόλων Προφητῶν, καὶ ἁπάσης τῆς κτίσεως, δεχομένου τε, ἀκηράτοις παλάμαις τοῦ Υἱοῦ σου, τὴν ἀμώμητον ψυχήν σου, Παρθενομῆτορ Θεόνυμφε."

meMA.LaudsGlory.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.LaudsGlory.melody = ""
meMA.LaudsGlory.incipit = "Τῇ ἀθανάτῳ σου Κοιμήσει…"
meMA.LaudsGlory.text = "Τῇ ἀθανάτῳ σου Κοιμήσει, Θεοτόκε Μήτερ τῆς ζωῆς, νεφέλαι τοὺς Ἀποστόλους, αἰθερίους διήρπαζον· καὶ κοσμικῶς διεσπαρμένους, ὁμοχώρους παρέστησαν, τῷ ἀχράντῳ σου σώματι· οἳ καὶ κηδεύσαντες σεπτῶς, τὴν φωνὴν τοῦ Γαβριήλ, μελῳδοῦντες ἀνεβόων· Χαῖρε κεχαριτωμένη, Παρθένε Μήτηρ ἀνύμφευτε, ὁ Κύριος μετὰ σοῦ. Μεθʼ ὧν ὡς Υἱόν σου καὶ Θεὸν ἡμῶν, ἱκέτευε, σωθῆναι τὰς ψυχὰς ἡμῶν."

