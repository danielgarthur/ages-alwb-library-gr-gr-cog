meVE.Apolytikion2ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion2ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion2ger.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meVE.Apolytikion2ger.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meVE.Apolytikion2ger.incipit = "Ὡς ἔμπλεως χάριτος, τοῦ Τιρσηλίου…"
meVE.Apolytikion2ger.text = "Ὡς ἔμπλεως χάριτος, τοῦ Τιρσηλίου φωτός, τὸ σκεῦος ἐφώτισας, τῆς ἐκλογῆς τοῦ Χριστοῦ, Ἀνανία Ἀπόστολε· ὅθεν ἀνακηρύξας, εὐσεβείας τὸν λόγον, ἄθλοις ἐβεβαιώσω, τὴν σωτήριον χάριν· διʼ ἧς τοῖς σὲ εὐφημοῦσι, δίδου τὰ πρόσφορα."

meVE.Apolytikion3.mode = miscellanea_gr_GR_cog.misc.Mode8
meVE.Apolytikion3.melody = he.ga_gr_GR_cog.heGA.EnSoiPaterAkrivos.name
meVE.GeneralApolytikion3.name = "Ρωμανέ,"
meVE.MM.GeneralApolytikion3.text = properties_gr_GR_cog.media.key

meMA.C1.acrostic.title = "Ὁ Κανὼν τοῦ Ἀποστόλου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχὶς (ἄνευ τῶν Θεοτοκίων)·"
meMA.C1.acrostic.text = "Ἀνανίου με τὸ κλέος δοξαζέτω."
meMA.C1.poet = poets_gr_GR_cog.po.IoannouMonachou
meMA.C1.title = titles_gr_GR_cog.ti.CanonApostle
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_Apostle.text

